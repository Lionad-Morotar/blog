_file":575,"_extension":576},"gists",false,"","面向对象编程",{"type":392,"children":393,"toc":564},"root",[394,403,409,444,453,461,466,471,476,481,504,510,515,533,538,543,551,556],{"type":395,"tag":396,"props":397,"children":399},"element","h2",{"id":398},"uml",[400],{"type":401,"value":402},"text","UML",{"type":395,"tag":404,"props":405,"children":406},"p",{},[407],{"type":401,"value":408},"类之间的关系总的来说可以包含这几种：",{"type":395,"tag":410,"props":411,"children":412},"ul",{},[413,419,424,429,434,439],{"type":395,"tag":414,"props":415,"children":416},"li",{},[417],{"type":401,"value":418},"A依赖B：对类B的修改会影响到类A",{"type":395,"tag":414,"props":420,"children":421},{},[422],{"type":401,"value":423},"A关联B：对象A知道对象B，类A依赖于类B",{"type":395,"tag":414,"props":425,"children":426},{},[427],{"type":401,"value":428},"A聚合B：对象A由B构成，类A依赖于类B",{"type":395,"tag":414,"props":430,"children":431},{},[432],{"type":401,"value":433},"A组合B：对象A由B构成且管理B的生命周期，类A依赖于类B",{"type":395,"tag":414,"props":435,"children":436},{},[437],{"type":401,"value":438},"A实现B：对象A可视为对象B，类A定义了接口B声明的方法，类A依赖于类B",{"type":395,"tag":414,"props":440,"children":441},{},[442],{"type":401,"value":443},"A继承B：对象A可视为对象B，类A继承类B的接口和实现，还可以对其扩展，类A依赖于类B",{"type":395,"tag":404,"props":445,"children":446},{},[447],{"type":395,"tag":448,"props":449,"children":452},"img",{"alt":450,"src":451},"Class Relations","https://mgear-image.oss-cn-shanghai.aliyuncs.com/image/other/b3612320-5822-48fc-8041-ead5c269f8e5.svg",[],{"type":395,"tag":404,"props":454,"children":455},{},[456],{"type":395,"tag":448,"props":457,"children":460},{"alt":458,"src":459},"Object Relations","https://mgear-image.oss-cn-shanghai.aliyuncs.com/image/other/cff33e77-e9aa-48eb-ae0d-01dd1561abd0.svg",[],{"type":395,"tag":396,"props":462,"children":464},{"id":463},"设计原则",[465],{"type":401,"value":463},{"type":395,"tag":404,"props":467,"children":468},{},[469],{"type":401,"value":470},"什么是优秀的软件设计？",{"type":395,"tag":404,"props":472,"children":473},{},[474],{"type":401,"value":475},"显然，在不同的公司、不同的项目，软件设计并没有一个恒定的标准，不过总的来说我们会讨论代码复用以及代码的扩展性。",{"type":395,"tag":404,"props":477,"children":478},{},[479],{"type":401,"value":480},"有几个基础的设计准则能帮助你做到这两点：",{"type":395,"tag":410,"props":482,"children":483},{},[484,489,494,499],{"type":395,"tag":414,"props":485,"children":486},{},[487],{"type":401,"value":488},"封装变化：找到程序中易变的内容并将其和不变的内容区分开来",{"type":395,"tag":414,"props":490,"children":491},{},[492],{"type":401,"value":493},"面向接口：依赖抽象通常优于依赖具体",{"type":395,"tag":414,"props":495,"children":496},{},[497],{"type":401,"value":498},"优先组合：组合优于继承，因为继承有着挺多保守诟病之处",{"type":395,"tag":414,"props":500,"children":501},{},[502],{"type":401,"value":503},"SOLID原则",{"type":395,"tag":505,"props":506,"children":508},"h3",{"id":507},"组合优于继承",[509],{"type":401,"value":507},{"type":395,"tag":404,"props":511,"children":512},{},[513],{"type":401,"value":514},"继承通常会引发大量问题：",{"type":395,"tag":410,"props":516,"children":517},{},[518,523,528],{"type":395,"tag":414,"props":519,"children":520},{},[521],{"type":401,"value":522},"子类不能减少父类的接口，并且需要保证兼容",{"type":395,"tag":414,"props":524,"children":525},{},[526],{"type":401,"value":527},"继承使状态逃离了父类的封装，子类和父类关系过于紧密",{"type":395,"tag":414,"props":529,"children":530},{},[531],{"type":401,"value":532},"继承经常被滥用，导致平行继承体系的出现",{"type":395,"tag":404,"props":534,"children":535},{},[536],{"type":401,"value":537},"假设你在售卖一些计算机，这些计算机包含笔记本，台式电脑以及算盘！",{"type":395,"tag":404,"props":539,"children":540},{},[541],{"type":401,"value":542},"如果使用继承的思路组织代码，你很可能对得到以下结构：",{"type":395,"tag":404,"props":544,"children":545},{},[546],{"type":395,"tag":448,"props":547,"children":550},{"alt":548,"src":549},"Computor Structure V1","https://mgear-image.oss-cn-shanghai.aliyuncs.com/image/other/348faf14-874b-410d-b4b9-ff00b04ab14e.svg",[],{"type":395,"tag":404,"props":552,"children":553},{},[554],{"type":401,"value":555},"使用组合可以方便地把不同的“维度”之间的并行关联拆分出来，降低代码复杂度：",{"type":395,"tag":404,"props":557,"children":558},{},[559],{"type":395,"tag":448,"props":560,"children":563},{"alt":561,"src":562},"Computor Structure V2","https://mgear-image.oss-cn-shanghai.aliyuncs.com/image/other/6cbffe1a-8efd-4de6-9ed3-bfef8392bab5.svg",[],{"title":389,"searchDepth":565,"depth":565,"links":566},2,[567,568],{"id":398,"depth":565,"text":402},{"id":463,"depth":565,"text":463,"children":569},[570],{"id":507,"depth":571,"text":507},3,"markdown","content:7.gists:oo.md","content","7.gists/oo.md","md",[578,580],{"_path":278,"title":277,"description":579},"神经科学也即脑科学，是一门以脑结构和功能为研究对象的综合学科，横跨了多个学科领域。现代神经科学有三个主流分支：细胞和分子神经科学、认知神经科学和发育神经学。",{"_path":284,"title":283,"description":581},"https://stefanzweifel.dev/posts/2023/09/16/an-opinionated-personal-folder-structure",1710257711920]