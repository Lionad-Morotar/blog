/web-components",{"title":388,"_path":389},"Windows","/gists/windows",{"title":391,"_path":392},"查缺补漏","/gists/wrong",{"title":394,"_path":395,"children":396},"代码笔记 / Gists","/source-code",[397],{"title":398,"_path":395},"🚄 代码笔记",{"title":400,"_path":401,"children":402},"我 / Abount","/hire",[403],{"title":404,"_path":401},"📬 技术简历",{"title":406,"_path":407,"children":408},"todo","/todo",[409],{"title":406,"_path":407},{"_path":269,"_dir":411,"_draft":412,"_partial":412,"_locale":413,"title":268,"description":413,"body":414,"_type":1336,"_id":1337,"_source":1338,"_file":1339,"_stem":1340,"_extension":1341},"gists",false,"",{"type":415,"children":416,"toc":1332},"root",[417,425,432,489,495,505,510,523,531,549,557,562,570,603,611,616,625,633,845,858,882,905,926,949,970,991,1014,1035,1198,1327],{"type":418,"tag":419,"props":420,"children":422},"element","h1",{"id":421},"isux-遇见大数据可视化系列",[423],{"type":424,"value":268},"text",{"type":418,"tag":426,"props":427,"children":429},"h2",{"id":428},"source",[430],{"type":424,"value":431},"Source",{"type":418,"tag":433,"props":434,"children":435},"ul",{},[436,449,459,469,479],{"type":418,"tag":437,"props":438,"children":439},"li",{},[440],{"type":418,"tag":441,"props":442,"children":446},"a",{"href":443,"rel":444},"https://isux.tencent.com/articles/91.html",[445],"nofollow",[447],{"type":424,"value":448},"未来已来，变革中的数据可视化",{"type":418,"tag":437,"props":450,"children":451},{},[452],{"type":418,"tag":441,"props":453,"children":456},{"href":454,"rel":455},"https://isux.tencent.com/articles/90.html",[445],[457],{"type":424,"value":458},"数据可视化基础研究",{"type":418,"tag":437,"props":460,"children":461},{},[462],{"type":418,"tag":441,"props":463,"children":466},{"href":464,"rel":465},"https://isux.tencent.com/articles/89.html",[445],[467],{"type":424,"value":468},"来做一个数据可视化报表",{"type":418,"tag":437,"props":470,"children":471},{},[472],{"type":418,"tag":441,"props":473,"children":476},{"href":474,"rel":475},"https://isux.tencent.com/articles/79.html",[445],[477],{"type":424,"value":478},"可视化系统搭建",{"type":418,"tag":437,"props":480,"children":481},{},[482],{"type":418,"tag":441,"props":483,"children":486},{"href":484,"rel":485},"https://isux.tencent.com/articles/78.html",[445],[487],{"type":424,"value":488},"数据图表设计（一）",{"type":418,"tag":426,"props":490,"children":492},{"id":491},"notes",[493],{"type":424,"value":494},"Notes",{"type":418,"tag":496,"props":497,"children":498},"p",{},[499],{"type":418,"tag":500,"props":501,"children":502},"q",{},[503],{"type":424,"value":504},"世界上90％的数据是在过去几年内产生的事实上，过去三十年中，全世界的数据量大约每两年增加10倍。",{"type":418,"tag":496,"props":506,"children":507},{},[508],{"type":424,"value":509},"因为数据多且增量大，所以需要持续改进可视化形式。技术上来说，可视化形式逐渐从从过去的表格、信息图、图表，逐渐扩充了交互图表、VR 图表等形式。",{"type":418,"tag":433,"props":511,"children":512},{},[513],{"type":418,"tag":437,"props":514,"children":515},{},[516],{"type":418,"tag":441,"props":517,"children":520},{"href":518,"rel":519},"https://www.datavis.ca/milestones/",[445],[521],{"type":424,"value":522},"图表可视化发展史",{"type":418,"tag":496,"props":524,"children":525},{},[526],{"type":418,"tag":500,"props":527,"children":528},{},[529],{"type":424,"value":530},"大数据时代的数据可视化具有哪些特征，笔者大概将其整理归纳为以下三点。",{"type":418,"tag":433,"props":532,"children":533},{},[534,539,544],{"type":418,"tag":437,"props":535,"children":536},{},[537],{"type":424,"value":538},"直观：使用 3D、动效等加强视觉冲击增强图表的可认知性",{"type":418,"tag":437,"props":540,"children":541},{},[542],{"type":424,"value":543},"交互：通过互动改变数据的表现形式",{"type":418,"tag":437,"props":545,"children":546},{},[547],{"type":424,"value":548},"实时：在庞大的数据量及增量的背景下，数据的实时性尤为重要",{"type":418,"tag":496,"props":550,"children":551},{},[552],{"type":418,"tag":500,"props":553,"children":554},{},[555],{"type":424,"value":556},"为什么人们会对信息图的传播内容更有效的认知呢？",{"type":418,"tag":496,"props":558,"children":559},{},[560],{"type":424,"value":561},"主要原因是因为视觉是人类最强的信息输入方式，其次，信息图提供了特定的语境，能展示多个维度数值并且相互比较。",{"type":418,"tag":496,"props":563,"children":564},{},[565],{"type":418,"tag":500,"props":566,"children":567},{},[568],{"type":424,"value":569},"信息图的分类也逐步划分为：图解（Diagram） 、图表（Chart） 、 表格（Table） 、统计图（Graph） 、 地图（Map）和图形符号（Pictogram）这几部分。",{"type":418,"tag":433,"props":571,"children":572},{},[573,578,583,588,593,598],{"type":418,"tag":437,"props":574,"children":575},{},[576],{"type":424,"value":577},"图解 Diagram：主要运用插图对事物进行说明",{"type":418,"tag":437,"props":579,"children":580},{},[581],{"type":424,"value":582},"图表 Chart：运用图形、线条及插图等，阐明事物的相互关系",{"type":418,"tag":437,"props":584,"children":585},{},[586],{"type":424,"value":587},"表格 Table：根据特定信息标准进行区分，设置纵轴与横轴",{"type":418,"tag":437,"props":589,"children":590},{},[591],{"type":424,"value":592},"统计图 Graph：通过数值来表现变化趋势或进行比较",{"type":418,"tag":437,"props":594,"children":595},{},[596],{"type":424,"value":597},"地图 Map：描述在特定区域和空间里的位置关系",{"type":418,"tag":437,"props":599,"children":600},{},[601],{"type":424,"value":602},"图形符号 Pictogram：不使用文字，运用图画直接传达信息",{"type":418,"tag":496,"props":604,"children":605},{},[606],{"type":418,"tag":500,"props":607,"children":608},{},[609],{"type":424,"value":610},"我们来看一个数据：2017年1月28号，成都PM2.5值245，从这个数据里能看出什么",{"type":418,"tag":496,"props":612,"children":613},{},[614],{"type":424,"value":615},"作者通过这个例子展现了收集、清洗、设计这三个完整的流程。他先从环境检测总站获取了 1 月份的检测信息，清洗后获得整个月的 PM2.5 值，通过将 PM2.5 映射成天空的照片，最后线性排开。从最后的图表中可以准确并清晰的确认 1 月份中的好天气。",{"type":418,"tag":496,"props":617,"children":618},{},[619],{"type":418,"tag":620,"props":621,"children":624},"img",{"alt":622,"src":623},"pm25.in 已经关停了","https://mgear-image.oss-cn-shanghai.aliyuncs.com/image/other/20231029164117.png",[],{"type":418,"tag":496,"props":626,"children":627},{},[628],{"type":418,"tag":500,"props":629,"children":630},{},[631],{"type":424,"value":632},"数据可视化迭代过程",{"type":418,"tag":496,"props":634,"children":635},{},[636],{"type":418,"tag":637,"props":638,"children":646},"svg",{"id":639,"width":640,"xmlns":641,"xmlnsXLink":642,"height":643,"style":644,"viewBox":645},"mermaid-1698568127289","100%","http://www.w3.org/2000/svg","http://www.w3.org/1999/xlink","