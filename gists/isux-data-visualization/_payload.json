y":390,"_type":1311,"_id":1312,"_source":1313,"_file":1314,"_extension":1315},"gists",false,"",{"type":391,"children":392,"toc":1307},"root",[393,401,408,465,471,481,486,499,507,525,533,538,546,579,587,592,601,609,821,834,858,881,902,925,946,967,990,1011,1174,1302],{"type":394,"tag":395,"props":396,"children":398},"element","h1",{"id":397},"isux-遇见大数据可视化系列",[399],{"type":400,"value":247},"text",{"type":394,"tag":402,"props":403,"children":405},"h2",{"id":404},"source",[406],{"type":400,"value":407},"Source",{"type":394,"tag":409,"props":410,"children":411},"ul",{},[412,425,435,445,455],{"type":394,"tag":413,"props":414,"children":415},"li",{},[416],{"type":394,"tag":417,"props":418,"children":422},"a",{"href":419,"rel":420},"https://isux.tencent.com/articles/91.html",[421],"nofollow",[423],{"type":400,"value":424},"未来已来，变革中的数据可视化",{"type":394,"tag":413,"props":426,"children":427},{},[428],{"type":394,"tag":417,"props":429,"children":432},{"href":430,"rel":431},"https://isux.tencent.com/articles/90.html",[421],[433],{"type":400,"value":434},"数据可视化基础研究",{"type":394,"tag":413,"props":436,"children":437},{},[438],{"type":394,"tag":417,"props":439,"children":442},{"href":440,"rel":441},"https://isux.tencent.com/articles/89.html",[421],[443],{"type":400,"value":444},"来做一个数据可视化报表",{"type":394,"tag":413,"props":446,"children":447},{},[448],{"type":394,"tag":417,"props":449,"children":452},{"href":450,"rel":451},"https://isux.tencent.com/articles/79.html",[421],[453],{"type":400,"value":454},"可视化系统搭建",{"type":394,"tag":413,"props":456,"children":457},{},[458],{"type":394,"tag":417,"props":459,"children":462},{"href":460,"rel":461},"https://isux.tencent.com/articles/78.html",[421],[463],{"type":400,"value":464},"数据图表设计（一）",{"type":394,"tag":402,"props":466,"children":468},{"id":467},"notes",[469],{"type":400,"value":470},"Notes",{"type":394,"tag":472,"props":473,"children":474},"p",{},[475],{"type":394,"tag":476,"props":477,"children":478},"q",{},[479],{"type":400,"value":480},"世界上90％的数据是在过去几年内产生的事实上，过去三十年中，全世界的数据量大约每两年增加10倍。",{"type":394,"tag":472,"props":482,"children":483},{},[484],{"type":400,"value":485},"因为数据多且增量大，所以需要持续改进可视化形式。技术上来说，可视化形式逐渐从从过去的表格、信息图、图表，逐渐扩充了交互图表、VR 图表等形式。",{"type":394,"tag":409,"props":487,"children":488},{},[489],{"type":394,"tag":413,"props":490,"children":491},{},[492],{"type":394,"tag":417,"props":493,"children":496},{"href":494,"rel":495},"https://www.datavis.ca/milestones/",[421],[497],{"type":400,"value":498},"图表可视化发展史",{"type":394,"tag":472,"props":500,"children":501},{},[502],{"type":394,"tag":476,"props":503,"children":504},{},[505],{"type":400,"value":506},"大数据时代的数据可视化具有哪些特征，笔者大概将其整理归纳为以下三点。",{"type":394,"tag":409,"props":508,"children":509},{},[510,515,520],{"type":394,"tag":413,"props":511,"children":512},{},[513],{"type":400,"value":514},"直观：使用 3D、动效等加强视觉冲击增强图表的可认知性",{"type":394,"tag":413,"props":516,"children":517},{},[518],{"type":400,"value":519},"交互：通过互动改变数据的表现形式",{"type":394,"tag":413,"props":521,"children":522},{},[523],{"type":400,"value":524},"实时：在庞大的数据量及增量的背景下，数据的实时性尤为重要",{"type":394,"tag":472,"props":526,"children":527},{},[528],{"type":394,"tag":476,"props":529,"children":530},{},[531],{"type":400,"value":532},"为什么人们会对信息图的传播内容更有效的认知呢？",{"type":394,"tag":472,"props":534,"children":535},{},[536],{"type":400,"value":537},"主要原因是因为视觉是人类最强的信息输入方式，其次，信息图提供了特定的语境，能展示多个维度数值并且相互比较。",{"type":394,"tag":472,"props":539,"children":540},{},[541],{"type":394,"tag":476,"props":542,"children":543},{},[544],{"type":400,"value":545},"信息图的分类也逐步划分为：图解（Diagram） 、图表（Chart） 、 表格（Table） 、统计图（Graph） 、 地图（Map）和图形符号（Pictogram）这几部分。",{"type":394,"tag":409,"props":547,"children":548},{},[549,554,559,564,569,574],{"type":394,"tag":413,"props":550,"children":551},{},[552],{"type":400,"value":553},"图解 Diagram：主要运用插图对事物进行说明",{"type":394,"tag":413,"props":555,"children":556},{},[557],{"type":400,"value":558},"图表 Chart：运用图形、线条及插图等，阐明事物的相互关系",{"type":394,"tag":413,"props":560,"children":561},{},[562],{"type":400,"value":563