90,"_draft":391,"_partial":391,"_locale":392,"title":250,"description":392,"body":393,"_type":1314,"_id":1315,"_source":1316,"_file":1317,"_extension":1318},"gists",false,"",{"type":394,"children":395,"toc":1310},"root",[396,404,411,468,474,484,489,502,510,528,536,541,549,582,590,595,604,612,824,837,861,884,905,928,949,970,993,1014,1177,1305],{"type":397,"tag":398,"props":399,"children":401},"element","h1",{"id":400},"isux-遇见大数据可视化系列",[402],{"type":403,"value":250},"text",{"type":397,"tag":405,"props":406,"children":408},"h2",{"id":407},"source",[409],{"type":403,"value":410},"Source",{"type":397,"tag":412,"props":413,"children":414},"ul",{},[415,428,438,448,458],{"type":397,"tag":416,"props":417,"children":418},"li",{},[419],{"type":397,"tag":420,"props":421,"children":425},"a",{"href":422,"rel":423},"https://isux.tencent.com/articles/91.html",[424],"nofollow",[426],{"type":403,"value":427},"未来已来，变革中的数据可视化",{"type":397,"tag":416,"props":429,"children":430},{},[431],{"type":397,"tag":420,"props":432,"children":435},{"href":433,"rel":434},"https://isux.tencent.com/articles/90.html",[424],[436],{"type":403,"value":437},"数据可视化基础研究",{"type":397,"tag":416,"props":439,"children":440},{},[441],{"type":397,"tag":420,"props":442,"children":445},{"href":443,"rel":444},"https://isux.tencent.com/articles/89.html",[424],[446],{"type":403,"value":447},"来做一个数据可视化报表",{"type":397,"tag":416,"props":449,"children":450},{},[451],{"type":397,"tag":420,"props":452,"children":455},{"href":453,"rel":454},"https://isux.tencent.com/articles/79.html",[424],[456],{"type":403,"value":457},"可视化系统搭建",{"type":397,"tag":416,"props":459,"children":460},{},[461],{"type":397,"tag":420,"props":462,"children":465},{"href":463,"rel":464},"https://isux.tencent.com/articles/78.html",[424],[466],{"type":403,"value":467},"数据图表设计（一）",{"type":397,"tag":405,"props":469,"children":471},{"id":470},"notes",[472],{"type":403,"value":473},"Notes",{"type":397,"tag":475,"props":476,"children":477},"p",{},[478],{"type":397,"tag":479,"props":480,"children":481},"q",{},[482],{"type":403,"value":483},"世界上90％的数据是在过去几年内产生的事实上，过去三十年中，全世界的数据量大约每两年增加10倍。",{"type":397,"tag":475,"props":485,"children":486},{},[487],{"type":403,"value":488},"因为数据多且增量大，所以需要持续改进可视化形式。技术上来说，可视化形式逐渐从从过去的表格、信息图、图表，逐渐扩充了交互图表、VR 图表等形式。",{"type":397,"tag":412,"props":490,"children":491},{},[492],{"type":397,"tag":416,"props":493,"children":494},{},[495],{"type":397,"tag":420,"props":496,"children":499},{"href":497,"rel":498},"https://www.datavis.ca/milestones/",[424],[500],{"type":403,"value":501},"图表可视化发展史",{"type":397,"tag":475,"props":503,"children":504},{},[505],{"type":397,"tag":479,"props":506,"children":507},{},[508],{"type":403,"value":509},"大数据时代的数据可视化具有哪些特征，笔者大概将其整理归纳为以下三点。",{"type":397,"tag":412,"props":511,"children":512},{},[513,518,523],{"type":397,"tag":416,"props":514,"children":515},{},[516],{"type":403,"value":517},"直观：使用 3D、动效等加强视觉冲击增强图表的可认知性",{"type":397,"tag":416,"props":519,"children":520},{},[521],{"type":403,"value":522},"交互：通过互动改变数据的表现形式",{"type":397,"tag":416,"props":524,"children":525},{},[526],{"type":403,"value":527},"实时：在庞大的数据量及增量的背景下，数据的实时性尤为重要",{"type":397,"tag":475,"props":529,"children":530},{},[531],{"type":397,"tag":479,"props":532,"children":533},{},[534],{"type":403,"value":535},"为什么人们会对信息图的传播内容更有效的认知呢？",{"type":397,"tag":475,"props":537,"children":538},{},[539],{"type":403,"value":540},"主要原因是因为视觉是人类最强的信息输入方式，其次，信息图提供了特定的语境，能展示多个维度数值并且相互比较。",{"type":397,"tag":475,"props":542,"children":543},{},[544],{"type":397,"tag":479,"props":545,"children":546},{},[547],{"type":403,"value":548},"信息图的分类也逐步划分为：图解（Diagram） 、图表（Chart） 、 表格（Table） 、统计图（Graph） 、 地图（Map）和图形符号（Pictogram）这几部分。",{"type":397,"tag":412,"props":550,"children":551},{},[552,557,562,567,572,577],{"type":397,"tag":416,"props":553,"children":554},{},[555],{"type":403,"value":556},"图解 Diagram：主要运用插图对事物进行说明",{"type":397,"tag":416,"props":558,"children":559},{},[560],{"type":403,"value":561},"图表 Chart：运用图形、线条及插图等，阐明事物的相互关系",{"t