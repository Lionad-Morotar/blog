ile":1311,"_extension":1312},"gists",false,"",{"type":388,"children":389,"toc":1304},"root",[390,398,405,462,468,478,483,496,504,522,530,535,543,576,584,589,598,606,818,831,855,878,899,922,943,964,987,1008,1171,1299],{"type":391,"tag":392,"props":393,"children":395},"element","h1",{"id":394},"isux-遇见大数据可视化系列",[396],{"type":397,"value":244},"text",{"type":391,"tag":399,"props":400,"children":402},"h2",{"id":401},"source",[403],{"type":397,"value":404},"Source",{"type":391,"tag":406,"props":407,"children":408},"ul",{},[409,422,432,442,452],{"type":391,"tag":410,"props":411,"children":412},"li",{},[413],{"type":391,"tag":414,"props":415,"children":419},"a",{"href":416,"rel":417},"https://isux.tencent.com/articles/91.html",[418],"nofollow",[420],{"type":397,"value":421},"未来已来，变革中的数据可视化",{"type":391,"tag":410,"props":423,"children":424},{},[425],{"type":391,"tag":414,"props":426,"children":429},{"href":427,"rel":428},"https://isux.tencent.com/articles/90.html",[418],[430],{"type":397,"value":431},"数据可视化基础研究",{"type":391,"tag":410,"props":433,"children":434},{},[435],{"type":391,"tag":414,"props":436,"children":439},{"href":437,"rel":438},"https://isux.tencent.com/articles/89.html",[418],[440],{"type":397,"value":441},"来做一个数据可视化报表",{"type":391,"tag":410,"props":443,"children":444},{},[445],{"type":391,"tag":414,"props":446,"children":449},{"href":447,"rel":448},"https://isux.tencent.com/articles/79.html",[418],[450],{"type":397,"value":451},"可视化系统搭建",{"type":391,"tag":410,"props":453,"children":454},{},[455],{"type":391,"tag":414,"props":456,"children":459},{"href":457,"rel":458},"https://isux.tencent.com/articles/78.html",[418],[460],{"type":397,"value":461},"数据图表设计（一）",{"type":391,"tag":399,"props":463,"children":465},{"id":464},"notes",[466],{"type":397,"value":467},"Notes",{"type":391,"tag":469,"props":470,"children":471},"p",{},[472],{"type":391,"tag":473,"props":474,"children":475},"q",{},[476],{"type":397,"value":477},"世界上90％的数据是在过去几年内产生的事实上，过去三十年中，全世界的数据量大约每两年增加10倍。",{"type":391,"tag":469,"props":479,"children":480},{},[481],{"type":397,"value":482},"因为数据多且增量大，所以需要持续改进可视化形式。技术上来说，可视化形式逐渐从从过去的表格、信息图、图表，逐渐扩充了交互图表、VR 图表等形式。",{"type":391,"tag":406,"props":484,"children":485},{},[486],{"type":391,"tag":410,"props":487,"children":488},{},[489],{"type":391,"tag":414,"props":490,"children":493},{"href":491,"rel":492},"https://www.datavis.ca/milestones/",[418],[494],{"type":397,"value":495},"图表可视化发展史",{"type":391,"tag":469,"props":497,"children":498},{},[499],{"type":391,"tag":473,"props":500,"children":501},{},[502],{"type":397,"value":503},"大数据时代的数据可视化具有哪些特征，笔者大概将其整理归纳为以下三点。",{"type":391,"tag":406,"props":505,"children":506},{},[507,512,517],{"type":391,"tag":410,"props":508,"children":509},{},[510],{"type":397,"value":511},"直观：使用 3D、动效等加强视觉冲击增强图表的可认知性",{"type":391,"tag":410,"props":513,"children":514},{},[515],{"type":397,"value":516},"交互：通过互动改变数据的表现形式",{"type":391,"tag":410,"props":518,"children":519},{},[520],{"type":397,"value":521},"实时：在庞大的数据量及增量的背景下，数据的实时性尤为重要",{"type":391,"tag":469,"props":523,"children":524},{},[525],{"type":391,"tag":473,"props":526,"children":527},{},[528],{"type":397,"value":529},"为什么人们会对信息图的传播内容更有效的认知呢？",{"type":391,"tag":469,"props":531,"children":532},{},[533],{"type":397,"value":534},"主要原因是因为视觉是人类最强的信息输入方式，其次，信息图提供了特定的语境，能展示多个维度数值并且相互比较。",{"type":391,"tag":469,"props":536,"children":537},{},[538],{"type":391,"tag":473,"props":539,"children":540},{},[541],{"type":397,"value":542},"信息图的分类也逐步划分为：图解（Diagram） 、图表（Chart） 、 表格（Table） 、统计图（Graph） 、 地图（Map）和图形符号（Pictogram）这几部分。",{"type":391,"tag":406,"props":544,"children":545},{},[546,551,556,561,566,571],{"type":391,"tag":410,"props":547,"children":548},{},[549],{"type":397,"value":550},"图解 Diagram：主要运用插图对事物进行说明",{"type":391,"tag":410,"props":552,"children":553},{},[554],{"type":397,"value":555},"图表 Chart：运用图形、线条及插图等，阐明事物的相互关系",{"type":391,"tag":410,"props":557,"children":558},{},[559],{"type":397,"value":560},"表格 Table：根据特定信息标准进行区分，设置纵轴与横轴",{"type":391,"ta