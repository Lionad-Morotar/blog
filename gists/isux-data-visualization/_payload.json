","/gists/wrong",{"title":388,"_path":389,"children":390},"我 / About","/hire",[391],{"title":392,"_path":389},"📬 技术简历",{"_path":266,"_dir":394,"_draft":395,"_partial":395,"_locale":396,"title":265,"description":396,"body":397,"_type":1319,"_id":1320,"_source":1321,"_file":1322,"_stem":1323,"_extension":1324},"gists",false,"",{"type":398,"children":399,"toc":1315},"root",[400,408,415,472,478,488,493,506,514,532,540,545,553,586,594,599,608,616,828,841,865,888,909,932,953,974,997,1018,1181,1310],{"type":401,"tag":402,"props":403,"children":405},"element","h1",{"id":404},"isux-遇见大数据可视化系列",[406],{"type":407,"value":265},"text",{"type":401,"tag":409,"props":410,"children":412},"h2",{"id":411},"source",[413],{"type":407,"value":414},"Source",{"type":401,"tag":416,"props":417,"children":418},"ul",{},[419,432,442,452,462],{"type":401,"tag":420,"props":421,"children":422},"li",{},[423],{"type":401,"tag":424,"props":425,"children":429},"a",{"href":426,"rel":427},"https://isux.tencent.com/articles/91.html",[428],"nofollow",[430],{"type":407,"value":431},"未来已来，变革中的数据可视化",{"type":401,"tag":420,"props":433,"children":434},{},[435],{"type":401,"tag":424,"props":436,"children":439},{"href":437,"rel":438},"https://isux.tencent.com/articles/90.html",[428],[440],{"type":407,"value":441},"数据可视化基础研究",{"type":401,"tag":420,"props":443,"children":444},{},[445],{"type":401,"tag":424,"props":446,"children":449},{"href":447,"rel":448},"https://isux.tencent.com/articles/89.html",[428],[450],{"type":407,"value":451},"来做一个数据可视化报表",{"type":401,"tag":420,"props":453,"children":454},{},[455],{"type":401,"tag":424,"props":456,"children":459},{"href":457,"rel":458},"https://isux.tencent.com/articles/79.html",[428],[460],{"type":407,"value":461},"可视化系统搭建",{"type":401,"tag":420,"props":463,"children":464},{},[465],{"type":401,"tag":424,"props":466,"children":469},{"href":467,"rel":468},"https://isux.tencent.com/articles/78.html",[428],[470],{"type":407,"value":471},"数据图表设计（一）",{"type":401,"tag":409,"props":473,"children":475},{"id":474},"notes",[476],{"type":407,"value":477},"Notes",{"type":401,"tag":479,"props":480,"children":481},"p",{},[482],{"type":401,"tag":483,"props":484,"children":485},"q",{},[486],{"type":407,"value":487},"世界上90％的数据是在过去几年内产生的事实上，过去三十年中，全世界的数据量大约每两年增加10倍。",{"type":401,"tag":479,"props":489,"children":490},{},[491],{"type":407,"value":492},"因为数据多且增量大，所以需要持续改进可视化形式。技术上来说，可视化形式逐渐从从过去的表格、信息图、图表，逐渐扩充了交互图表、VR 图表等形式。",{"type":401,"tag":416,"props":494,"children":495},{},[496],{"type":401,"tag":420,"props":497,"children":498},{},[499],{"type":401,"tag":424,"props":500,"children":503},{"href":501,"rel":502},"https://www.datavis.ca/milestones/",[428],[504],{"type":407,"value":505},"图表可视化发展史",{"type":401,"tag":479,"props":507,"children":508},{},[509],{"type":401,"tag":483,"props":510,"children":511},{},[512],{"type":407,"value":513},"大数据时代的数据可视化具有哪些特征，笔者大概将其整理归纳为以下三点。",{"type":401,"tag":416,"props":515,"children":516},{},[517,522,527],{"type":401,"tag":420,"props":518,"children":519},{},[520],{"type":407,"value":521},"直观：使用 3D、动效等加强视觉冲击增强图表的可认知性",{"type":401,"tag":420,"props":523,"children":524},{},[525],{"type":407,"value":526},"交互：通过互动改变数据的表现形式",{"type":401,"tag":420,"props":528,"children":529},{},[530],{"type":407,"value":531},"实时：在庞大的数据量及增量的背景下，数据的实时性尤为重要",{"type":401,"tag":479,"props":533,"children":534},{},[535],{"type":401,"tag":483,"props":536,"children":537},{},[538],{"type":407,"value":539},"为什么人们会对信息图的传播内容更有效的认知呢？",{"type":401,"tag":479,"props":541,"children":542},{},[543],{"type":407,"value":544},"主要原因是因为视觉是人类最强的信息输入方式，其次，信息图提供了特定的语境，能展示多个维度数值并且相互比较。",{"type":401,"tag":479,"props":546,"children":547},{},[548],{"type":401,"tag":483,"props":549,"children":550},{},[551],{"type":407,"value":552},"信息图的分类也逐步划分为：图解（Diagram） 、图表（Chart） 、 表格（Table） 、统计图（Graph） 、 地图（Map）和图形符号（Pictogram）这几部分。",{"type":401,"tag":416,"props":554,"children":555},{},[556,561,566,571,576,581],{"type":401,"tag":420,"props":557,"children":558},{},[559],{"type":407,"value"