:393,"_locale":394,"title":265,"description":394,"body":395,"_type":515,"_id":516,"_source":517,"_file":518,"_extension":519},"/maps/_web/miniapp","_web",false,true,"",{"type":396,"children":397,"toc":510},"root",[398,405,411,438,445,451,457,462,468,473,478,484,498],{"type":399,"tag":400,"props":401,"children":402},"element","h1",{"id":265},[403],{"type":404,"value":265},"text",{"type":399,"tag":406,"props":407,"children":409},"h2",{"id":408},"技术",[410],{"type":404,"value":408},{"type":399,"tag":412,"props":413,"children":414},"ul",{},[415,428],{"type":399,"tag":416,"props":417,"children":418},"li",{},[419],{"type":399,"tag":420,"props":421,"children":425},"a",{"href":422,"rel":423},"https://developers.weixin.qq.com/ebook?action=get_post_info&volumn=1&lang=zh_CN&book=miniprogram&docid=0000e82f924ca0bb00869a5de5ec0a",[424],"nofollow",[426],{"type":404,"value":427},"小程序底层框架",{"type":399,"tag":416,"props":429,"children":430},{},[431],{"type":399,"tag":420,"props":432,"children":435},{"href":433,"rel":434},"https://zhaomenghuan.js.org/blog/wechat-miniprogram-principle-analysis.html",[424],[436],{"type":404,"value":437},"微信小程序技术原理分析 | 框架逆向",{"type":399,"tag":439,"props":440,"children":442},"h4",{"id":441},"小程序双线程模型是怎样的",[443],{"type":404,"value":444},"小程序双线程模型是怎样的？",{"type":399,"tag":446,"props":447,"children":448},"p",{},[449],{"type":404,"value":450},"小程序的渲染层和逻辑层使用两个不同的线程管理。渲染层使用 Webviews 渲染页面，逻辑层使用 JSCore 解析执行 JS，而两个线程间的通讯以及 JS 发起的 HTTP、WS 等请求交由 Native 处理。",{"type":399,"tag":439,"props":452,"children":454},{"id":453},"小程序对比网页的优势与限制",[455],{"type":404,"value":456},"小程序对比网页的优势与限制？",{"type":399,"tag":446,"props":458,"children":459},{},[460],{"type":404,"value":461},"相比 PWA 的手动缓存，小程序框架提供了包的自动更新机制。但是出于安全和管控的考虑，小程序阻止页面跳转、动态脚本以及 DOM 操作等能力。",{"type":399,"tag":439,"props":463,"children":465},{"id":464},"小程序的-exparser-是什么",[466],{"type":404,"value":467},"小程序的 Exparser 是什么？",{"type":399,"tag":446,"props":469,"children":470},{},[471],{"type":404,"value":472},"Exparser 是一套小程序的组件框架，它提供了一套类似 Shadow DOM 标准的实现，以维护页面节点树信息，同时它提供了一套内置的基础组件库，用于满足小程序日常开发需求。",{"type":399,"tag":406,"props":474,"children":476},{"id":475},"常见问题",[477],{"type":404,"value":475},{"type":399,"tag":439,"props":479,"children":481},{"id":480},"分包异步化是什么",[482],{"type":404,"value":483},"分包异步化是什么？",{"type":399,"tag":446,"props":485,"children":486},{},[487,489,496],{"type":404,"value":488},"指通过 ",{"type":399,"tag":490,"props":491,"children":493},"code",{"className":492},[],[494],{"type":404,"value":495},"require.async('sub/xxx')",{"type":404,"value":497}," 引用分包中的代码，达到减小主包体积的目的。同理，还有分包插件异步化这种方法。",{"type":399,"tag":446,"props":499,"children":500},{},[501,503],{"type":404,"value":502},"见：",{"type":399,"tag":420,"props":504,"children":507},{"href":505,"rel":506},"https://juejin.cn/post/7205092873326723109",[424],[508],{"type":404,"value":509},"分包异步化在货拉拉微信小程序中的实践",{"title":394,"searchDepth":511,"depth":511,"links":512},2,[513,514],{"id":408,"depth":511,"text":408},{"id":475,"depth":511,"text":475},"markdown","content:6.maps:_web:miniapp.md","content","6.maps/_web/miniapp.md","md",[521,524],{"_path":522,"title":523,"description":394},"/maps/_web/crawler","爬虫与反爬虫",{"_path":525,"title":526,"description":394},"/maps/_web/performance","页面性能",1718183410048]