le":388,"_path":389,"children":390},"我 / About","/hire",[391],{"title":392,"_path":389},"📬 技术简历",{"_path":394,"_dir":395,"_draft":396,"_partial":397,"_locale":398,"title":399,"description":400,"body":401,"_type":537,"_id":538,"_source":539,"_file":540,"_stem":541,"_extension":542},"/maps/_server/nodejs","_server",false,true,"","NodeJS","NodeJS 是一个非阻塞 IO、事件驱动的服务端 JS 运行环境。",{"type":402,"children":403,"toc":530},"root",[404,412,419,424,430,435,441,446,452,458,463,469,474,480,485,491,497,502,516,521],{"type":405,"tag":406,"props":407,"children":409},"element","h2",{"id":408},"简介",[410],{"type":411,"value":408},"text",{"type":405,"tag":413,"props":414,"children":416},"h4",{"id":415},"nodejs-是什么",[417],{"type":411,"value":418},"NodeJS 是什么？",{"type":405,"tag":420,"props":421,"children":422},"p",{},[423],{"type":411,"value":400},{"type":405,"tag":413,"props":425,"children":427},{"id":426},"非阻塞-io-是什么",[428],{"type":411,"value":429},"非阻塞 IO 是什么？",{"type":405,"tag":420,"props":431,"children":432},{},[433],{"type":411,"value":434},"非阻塞 IO 意味着在执行 IO 操作时，将耗时任务以异步的方式交给子线程处理，等完成之后再通过回调的方式继续执行后续代码。",{"type":405,"tag":413,"props":436,"children":438},{"id":437},"事件循环机制和浏览器中有什么不同",[439],{"type":411,"value":440},"事件循环机制和浏览器中有什么不同？",{"type":405,"tag":420,"props":442,"children":443},{},[444],{"type":411,"value":445},"TODO，类似但稍有区别。",{"type":405,"tag":406,"props":447,"children":449},{"id":448},"v8",[450],{"type":411,"value":451},"V8",{"type":405,"tag":413,"props":453,"children":455},{"id":454},"引擎的垃圾回收机制有了解吗",[456],{"type":411,"value":457},"引擎的垃圾回收机制有了解吗？",{"type":405,"tag":420,"props":459,"children":460},{},[461],{"type":411,"value":462},"V8 使用分代回收机制，将内存分为新生代和老生代空间，分别用不同的算法进行 GC。新生代空间每个周期都会触发 ScavengeGC，俗称小周期。如果两个周期内都没被清除的变量则转移到老生代内存中。老生代空间则使用标记清除算法（和标记紧缩算法）进行清理。",{"type":405,"tag":413,"props":464,"children":466},{"id":465},"新生代空间的-gc-过程是怎样的",[467],{"type":411,"value":468},"新生代空间的 GC 过程是怎样的？",{"type":405,"tag":420,"props":470,"children":471},{},[472],{"type":411,"value":473},"新生代空间通常只有 1-8 MB，被划分为两个等大的区域，出区和入区。新增变量放到入区中。一但入区满了，便把活跃对象复制到出区中，删除不活跃对象（紧缩过程）并交换出区入区，如此称为一个周期。如果活跃对象再两个周期内都没有被清除，则转移只老生代内存中。",{"type":405,"tag":413,"props":475,"children":477},{"id":476},"老生代空间的-gc-过程是怎样的",[478],{"type":411,"value":479},"老生代空间的 GC 过程是怎样的？",{"type":405,"tag":420,"props":481,"children":482},{},[483],{"type":411,"value":484},"老生代空间的 GC 分标记和清除（或紧缩）两个阶段。每次循环，标记算法会将一个对象从队列中取出，将它以及邻接对象标记。到了清除阶段，未被标记的对象都将被删除。如果清楚阶段额外执行紧缩过程，则会尝试将零散的对象移动成连续的整体，以解决内存碎片化问题。为了优化老生代空间 GC 的“全停顿”，V8 将老生代空间的标记过程拆分为了子步骤，这样就可以将标记算法穿插在其它任务过程中执行。",{"type":405,"tag":406,"props":486,"children":488},{"id":487},"api",[489],{"type":411,"value":490},"API",{"type":405,"tag":413,"props":492,"children":494},{"id":493},"nodevm-是什么",[495],{"type":411,"value":496},"node:vm 是什么？",{"type":405,"tag":420,"props":498,"children":499},{},[500],{"type":411,"value":501},"node:vm 模块允许在 V8 虚拟机上下文中编译和运行代码，上下文中的任何属性都被视为全局变量，对全局变量任何改动也反映到上下文对象中。但是 node:vm 不是一个安全机制，不应该使用它来运行不受信任的代码。",{"type":405,"tag":420,"props":503,"children":504},{},[505,507],{"type":411,"value":506},"见：",{"type":405,"tag":508,"props":509,"children":513},"a",{"href":510,"rel":511},"http://nodejs.cn/api/vm.html#vm_vm_executing_javascript",[512],"nofollow",[514],{"type":411,"value":515},"VM",{"type":405,"tag":406,"props":517,"children":519},{"id":518},"源码解析",[520],{"type":411,"value":518},{"type":405,"tag":420,"props":522,"children":523},{},[524],{"type":405,"tag":508,"props":525,"children":527},{"href":526},"/maps/_server/nodejs/source/require",[528],{"type":411,"value":529},"NodeJS | require",{"title":398,"searchDepth":531,"depth":531,"links":532},2,[533,534,535,536],{"id":408,"depth":531,"text":408},{"id":448,"depth":531,"text":451},{"id":487,"depth":531,"text":490},{"id":518,"depth":531,"text":518},"markdown","content:6.maps:_server:nodejs:index.md","content","6.maps/_server/nodejs/index.md","6.maps/_server/nodejs/index","md",[544,548],{"_path":545,"title":546,"description":547},"/maps/_schema/zod","Zod","Zod 是一个 TypeScript-first 的验证库。",{"_path":526,"title":549,"description":550},"NodeJS Require","NodeJS 模块导入的源码解析",1741604955231]