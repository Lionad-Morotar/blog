le":388,"_path":389},"Windows","/gists/windows",{"title":391,"_path":392},"查缺补漏","/gists/wrong",{"title":394,"_path":395,"children":396},"代码笔记 / Gists","/source-code",[397],{"title":398,"_path":395},"🚄 代码笔记",{"title":400,"_path":401,"children":402},"我 / Abount","/hire",[403],{"title":404,"_path":401},"📬 技术简历",{"title":406,"_path":407,"children":408},"todo","/todo",[409],{"title":406,"_path":407},{"_path":411,"_dir":412,"_draft":413,"_partial":414,"_locale":415,"title":416,"description":417,"body":418,"_type":554,"_id":555,"_source":556,"_file":557,"_stem":558,"_extension":559},"/maps/_server/nodejs","_server",false,true,"","NodeJS","NodeJS 是一个非阻塞 IO、事件驱动的服务端 JS 运行环境。",{"type":419,"children":420,"toc":547},"root",[421,429,436,441,447,452,458,463,469,475,480,486,491,497,502,508,514,519,533,538],{"type":422,"tag":423,"props":424,"children":426},"element","h2",{"id":425},"简介",[427],{"type":428,"value":425},"text",{"type":422,"tag":430,"props":431,"children":433},"h4",{"id":432},"nodejs-是什么",[434],{"type":428,"value":435},"NodeJS 是什么？",{"type":422,"tag":437,"props":438,"children":439},"p",{},[440],{"type":428,"value":417},{"type":422,"tag":430,"props":442,"children":444},{"id":443},"非阻塞-io-是什么",[445],{"type":428,"value":446},"非阻塞 IO 是什么？",{"type":422,"tag":437,"props":448,"children":449},{},[450],{"type":428,"value":451},"非阻塞 IO 意味着在执行 IO 操作时，将耗时任务以异步的方式交给子线程处理，等完成之后再通过回调的方式继续执行后续代码。",{"type":422,"tag":430,"props":453,"children":455},{"id":454},"事件循环机制和浏览器中有什么不同",[456],{"type":428,"value":457},"事件循环机制和浏览器中有什么不同？",{"type":422,"tag":437,"props":459,"children":460},{},[461],{"type":428,"value":462},"TODO，类似但稍有区别。",{"type":422,"tag":423,"props":464,"children":466},{"id":465},"v8",[467],{"type":428,"value":468},"V8",{"type":422,"tag":430,"props":470,"children":472},{"id":471},"引擎的垃圾回收机制有了解吗",[473],{"type":428,"value":474},"引擎的垃圾回收机制有了解吗？",{"type":422,"tag":437,"props":476,"children":477},{},[478],{"type":428,"value":479},"V8 使用分代回收机制，将内存分为新生代和老生代空间，分别用不同的算法进行 GC。新生代空间每个周期都会触发 ScavengeGC，俗称小周期。如果两个周期内都没被清除的变量则转移到老生代内存中。老生代空间则使用标记清除算法（和标记紧缩算法）进行清理。",{"type":422,"tag":430,"props":481,"children":483},{"id":482},"新生代空间的-gc-过程是怎样的",[484],{"type":428,"value":485},"新生代空间的 GC 过程是怎样的？",{"type":422,"tag":437,"props":487,"children":488},{},[489],{"type":428,"value":490},"新生代空间通常只有 1-8 MB，被划分为两个等大的区域，出区和入区。新增变量放到入区中。一但入区满了，便把活跃对象复制到出区中，删除不活跃对象（紧缩过程）并交换出区入区，如此称为一个周期。如果活跃对象再两个周期内都没有被清除，则转移只老生代内存中。",{"type":422,"tag":430,"props":492,"children":494},{"id":493},"老生代空间的-gc-过程是怎样的",[495],{"type":428,"value":496},"老生代空间的 GC 过程是怎样的？",{"type":422,"tag":437,"props":498,"children":499},{},[500],{"type":428,"value":501},"老生代空间的 GC 分标记和清除（或紧缩）两个阶段。每次循环，标记算法会将一个对象从队列中取出，将它以及邻接对象标记。到了清除阶段，未被标记的对象都将被删除。如果清楚阶段额外执行紧缩过程，则会尝试将零散的对象移动成连续的整体，以解决内存碎片化问题。为了优化老生代空间 GC 的“全停顿”，V8 将老生代空间的标记过程拆分为了子步骤，这样就可以将标记算法穿插在其它任务过程中执行。",{"type":422,"tag":423,"props":503,"children":505},{"id":504},"api",[506],{"type":428,"value":507},"API",{"type":422,"tag":430,"props":509,"children":511},{"id":510},"nodevm-是什么",[512],{"type":428,"value":513},"node:vm 是什么？",{"type":422,"tag":437,"props":515,"children":516},{},[517],{"type":428,"value":518},"node:vm 模块允许在 V8 虚拟机上下文中编译和运行代码，上下文中的任何属性都被视为全局变量，对全局变量任何改动也反映到上下文对象中。但是 node:vm 不是一个安全机制，不应该使用它来运行不受信任的代码。",{"type":422,"tag":437,"props":520,"children":521},{},[522,524],{"type":428,"value":523},"见：",{"type":422,"tag":525,"props":526,"children":530},"a",{"href":527,"rel":528},"http://nodejs.cn/api/vm.html#vm_vm_executing_javascript",[529],"nofollow",[531],{"type":428,"value":532},"VM",{"type":422,"tag":423,"props":534,"children":536},{"id":535},"源码解析",[537],{"type":428,"value":535},{"type":422,"tag":437,"props":539,"children":540},{},[541],{"type":422,"tag":525,"props":542,"children":544},{"href":543},"/maps/_server/nodejs/source/require",[545],{"type":428,"value":546},"NodeJS | require",{"title":415,"searchDepth":548,"depth":548,"links":549},2,[550,551,552,553],{"id":425,"depth":548,"text":425},{"id":465,"depth":548,"text":468},{"id":504,"depth":548,"text":507},{"id":535,"depth":548,"text":535},"markdown","content:6.maps:_server:nodejs:index.md","content","6.maps/_server/nodejs/index.md","6.maps/_server/nodejs/index","md",[561,565],{"_path":562,"title":563,"description":564},"/maps/_photography","摄影","技术传达情感，摄影记录生活。",{"_path":543,"title":566,"description":567},"NodeJS Require","NodeJS 模块导入的源码解析",1732808118631]