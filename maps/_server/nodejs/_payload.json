":177},"element","h2",{"id":176},"简介",[178],{"type":179,"value":176},"text",{"type":173,"tag":181,"props":182,"children":184},"h4",{"id":183},"nodejs-是什么",[185],{"type":179,"value":186},"NodeJS 是什么？",{"type":173,"tag":188,"props":189,"children":190},"p",{},[191],{"type":179,"value":168},{"type":173,"tag":181,"props":193,"children":195},{"id":194},"非阻塞-io-是什么",[196],{"type":179,"value":197},"非阻塞 IO 是什么？",{"type":173,"tag":188,"props":199,"children":200},{},[201],{"type":179,"value":202},"非阻塞 IO 意味着在执行 IO 操作时，将耗时任务以异步的方式交给子线程处理，等完成之后再通过回调的方式继续执行后续代码。",{"type":173,"tag":181,"props":204,"children":206},{"id":205},"事件循环机制和浏览器中有什么不同",[207],{"type":179,"value":208},"事件循环机制和浏览器中有什么不同？",{"type":173,"tag":188,"props":210,"children":211},{},[212],{"type":179,"value":213},"TODO，类似但稍有区别。",{"type":173,"tag":181,"props":215,"children":217},{"id":216},"_2025-年现代-nodejs-模式",[218],{"type":179,"value":219},"《2025 年现代 Node.js 模式》",{"type":173,"tag":221,"props":222,"children":223},"ol",{},[224,239,244,249,254,259,264],{"type":173,"tag":225,"props":226,"children":227},"li",{},[228,230,237],{"type":179,"value":229},"Embrace Web Standards: Use ",{"type":173,"tag":231,"props":232,"children":234},"code",{"className":233},[],[235],{"type":179,"value":236},"node:",{"type":179,"value":238}," prefixes, fetch API, AbortController, and Web Streams for better compatibility and reduced dependencies",{"type":173,"tag":225,"props":240,"children":241},{},[242],{"type":179,"value":243},"Built-in Tools: The test runner, watch mode, and environment file support reduce external dependencies and configuration complexity",{"type":173,"tag":225,"props":245,"children":246},{},[247],{"type":179,"value":248},"Modern Async Patterns: Top-level await, structured error handling, and async iterators make code more readable and maintainable",{"type":173,"tag":225,"props":250,"children":251},{},[252],{"type":179,"value":253},"Worker Threads for CPU-intensive tasks: worker threads provide true parallelism without blocking the main thread",{"type":173,"tag":225,"props":255,"children":256},{},[257],{"type":179,"value":258},"Robust Application: Use permission models, diagnostics channels, and performance monitoring to build robust, observable applications",{"type":173,"tag":225,"props":260,"children":261},{},[262],{"type":179,"value":263},"Developer Experience: Watch mode, built-in testing, and import maps create a more pleasant development workflow",{"type":173,"tag":225,"props":265,"children":266},{},[267],{"type":179,"value":268},"Distribution: Single executable applications and modern packaging make deployment simpler",{"type":173,"tag":188,"props":270,"children":271},{},[272,274],{"type":179,"value":273},"见：",{"type":173,"tag":275,"props":276,"children":280},"a",{"href":277,"rel":278},"https://kashw1n.com/blog/nodejs-2025/",[279],"nofollow",[281],{"type":179,"value":282},"Modern Node.js Patterns for 2025",{"type":173,"tag":174,"props":284,"children":286},{"id":285},"v8",[287],{"type":179,"value":288},"V8",{"type":173,"tag":290,"props":291,"children":292},"ul",{},[293],{"type":173,"tag":225,"props":294,"children":295},{},[296],{"type":173,"tag":275,"props":297,"children":300},{"href":298,"rel":299},"https://v8.dev/docs",[279],[301],{"type":179,"value":302},"V8 Docs",{"type":173,"tag":181,"props":304,"children":306},{"id":305},"引擎的垃圾回收机制有了解吗",[307],{"type":179,"value":308},"引擎的垃圾回收机制有了解吗？",{"type":173,"tag":188,"props":310,"children":311},{},[312],{"type":179,"value":313},"V8 使用分代回收机制，将内存分为新生代和老生代空间，分别用不同的算法进行 GC。新生代空间每个周期都会触发 ScavengeGC，俗称小周期。如果两个周期内都没被清除的变量则转移到老生代内存中。老生代空间则使用标记清除算法（和标记紧缩算法）进行清理。",{"type":173,"tag":181,"props":315,"children":317},{"id":316},"新生代空间的-gc-过程是怎样的",[318],{"type":179,"value":319},"新生代空间的 GC 过程是怎样的？",{"type":173,"tag":188,"props":321,"children":322},{},[323],{"type":179,"value":324},"新生代空间通常只有 1-8 MB，被划分为两个等大的区域，出区和入区。新增变量放到入区中。一但入区满了，便把活跃对象复制到出区中，删除不活跃对象（紧缩过程）并交换出区入区，如此称为一个周期。如果活跃对象再两个周期内都没有被清除，则转移只老生代内存中。",{"type":173,"tag":181,"props":326,"children":328},{"id":327},"老生代空间的-gc-过程是怎样的",[329],{"type":179,"value":330},"老生代空间的 GC 过程是怎样的？",{"type":173,"tag":188,"props":332,"children":333},{},[334],{"type":179,"value":335},"老生代空间的 GC 分标记和清除（或紧缩）两个阶段。每次循环，标记算法会将一个对象从队列中取出，将它以及邻接对象标记。到了清除阶段，未被标记的对象都将被删除。如果清楚阶段额外执行紧缩过程，则会尝试将零散的对象移动成连续的整体，以解决内存碎片化问题。为了优化老生代空间 GC 的“全停顿”，V8 将老生代空间的标记过程拆分为了子步骤，这样就可以将标记算法穿插在其它任务过程中执行。",{"type":173,"tag":181,"props":337,"children":339},{"id":338},"常见的商业开源研究用的-js-引擎有哪些",[340],{"type":179,"value":341},"常见的商业、开源、研究用的 JS 引擎有哪些？",{"type":173,"tag":188,"props":343,"children":344},{},[345,347,358],{"type":179,"value":346},"见",{"type":173,"tag":348,"props":349,"children":350},"i",{},[351],{"type":173,"tag":275,"props":352,"children":355},{"href":353,"rel":354},"https://notes.eatonphil.com/javascript-implementations.html",[279],[356],{"type":179,"value":357},"Enumerating and analyzing 40+ non-V8 JavaScript implementations",{"type":179,"value":359},"，列举了 40 多个非 V8 的 JS 引擎实现。",{"type":173,"tag":174,"props":361,"children":363},{"id":362},"api",[364],{"type":179,"value":365},"API",{"type":173,"tag":181,"props":367,"children":369},{"id":368},"nodevm-是什么",[370],{"type":179,"value":371},"node:vm 是什么？",{"type":173,"tag":188,"props":373,"children":374},{},[375],{"type":179,"value":376},"node:vm 模块允许在 V8 虚拟机上下文中编译和运行代码，上下文中的任何属性都被视为全局变量，对全局变量任何改动也反映到上下文对象中。但是 node:vm 不是一个安全机制，不应该使用它来运行不受信任的代码。",{"type":173,"tag":188,"props":378,"children":379},{},[380,381],{"type":179,"value":273},{"type":173,"tag":275,"props":382,"children":385},{"href":383,"rel":384},"http://nodejs.cn/api/vm.html#vm_vm_executing_javascript",[279],[386],{"type":179,"value":387},"VM",{"type":173,"tag":174,"props":389,"children":391},{"id":390},"源码解析",[392],{"type":179,"value":390},{"type":173,"tag":188,"props":394,"children":395},{},[396],{"type":173,"tag":275,"props":397,"children":399},{"href":398},"/maps/_server/nodejs/source/require",[400],{"type":179,"value":401},"NodeJS | require",{"type":173,"tag":174,"props":403,"children":405},{"id":404},"调试",[406],{"type":179,"value":404},{"type":173,"tag":290,"props":408,"children":409},{},[410],{"type":173,"tag":225,"props":411,"children":412},{},[413],{"type":173,"tag":275,"props":414,"children":417},{"href":415,"rel":416},"https://juejin.cn/post/6844903904728236045",[279],[418],{"type":179,"value":419},"使用火焰图优化 NodeJS CPU 爆涨",{"title":166,"searchDepth":421,"depth":421,"links":422},2,[423,424,425,426,427],{"id":176,"depth":421,"text":176},{"id":285,"depth":421,"text":288},{"id":362,"depth":421,"text":365},{"id":390,"depth":421,"text":390},{"id":404,"depth":421,"text":404},"markdown","content:6.maps:_server:nodejs:index.md","content","6.maps/_server/nodejs/index.md","6.maps/_server/nodejs/index","md",[435,439],{"_path":436,"title":437,"description":438},"/maps/_seo/seo","搜索引擎优化","搜索引擎优化包含了对网站内容的基本元素的一系列改动，这些改动通常相互作用，提高用户体验以及增加网站在自然搜索结果中的排名。",{"_path":398,"title":440,"description":441},"NodeJS Require","NodeJS 模块导入的源码解析",1761757106507]