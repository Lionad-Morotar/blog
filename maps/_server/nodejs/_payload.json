6},"SS(SSR)","/gists/vpn",{"title":388,"_path":389},"VS Code 插件开发","/gists/vscode-plugin",{"title":391,"_path":392},"Web Components","/gists/web-components",{"title":394,"_path":395},"Windows","/gists/windows",{"title":397,"_path":398},"查缺补漏","/gists/wrong",{"title":400,"_path":401,"children":402},"代码笔记 / Gists","/source-code",[403],{"title":404,"_path":401},"🚄 代码笔记",{"title":406,"_path":407,"children":408},"我 / About","/hire",[409],{"title":410,"_path":407},"📬 技术简历",{"title":412,"_path":413,"children":414},"todo","/todo",[415],{"title":412,"_path":413},{"_path":417,"_dir":418,"_draft":419,"_partial":420,"_locale":421,"title":422,"description":423,"body":424,"_type":560,"_id":561,"_source":562,"_file":563,"_stem":564,"_extension":565},"/maps/_server/nodejs","_server",false,true,"","NodeJS","NodeJS 是一个非阻塞 IO、事件驱动的服务端 JS 运行环境。",{"type":425,"children":426,"toc":553},"root",[427,435,442,447,453,458,464,469,475,481,486,492,497,503,508,514,520,525,539,544],{"type":428,"tag":429,"props":430,"children":432},"element","h2",{"id":431},"简介",[433],{"type":434,"value":431},"text",{"type":428,"tag":436,"props":437,"children":439},"h4",{"id":438},"nodejs-是什么",[440],{"type":434,"value":441},"NodeJS 是什么？",{"type":428,"tag":443,"props":444,"children":445},"p",{},[446],{"type":434,"value":423},{"type":428,"tag":436,"props":448,"children":450},{"id":449},"非阻塞-io-是什么",[451],{"type":434,"value":452},"非阻塞 IO 是什么？",{"type":428,"tag":443,"props":454,"children":455},{},[456],{"type":434,"value":457},"非阻塞 IO 意味着在执行 IO 操作时，将耗时任务以异步的方式交给子线程处理，等完成之后再通过回调的方式继续执行后续代码。",{"type":428,"tag":436,"props":459,"children":461},{"id":460},"事件循环机制和浏览器中有什么不同",[462],{"type":434,"value":463},"事件循环机制和浏览器中有什么不同？",{"type":428,"tag":443,"props":465,"children":466},{},[467],{"type":434,"value":468},"TODO，类似但稍有区别。",{"type":428,"tag":429,"props":470,"children":472},{"id":471},"v8",[473],{"type":434,"value":474},"V8",{"type":428,"tag":436,"props":476,"children":478},{"id":477},"引擎的垃圾回收机制有了解吗",[479],{"type":434,"value":480},"引擎的垃圾回收机制有了解吗？",{"type":428,"tag":443,"props":482,"children":483},{},[484],{"type":434,"value":485},"V8 使用分代回收机制，将内存分为新生代和老生代空间，分别用不同的算法进行 GC。新生代空间每个周期都会触发 ScavengeGC，俗称小周期。如果两个周期内都没被清除的变量则转移到老生代内存中。老生代空间则使用标记清除算法（和标记紧缩算法）进行清理。",{"type":428,"tag":436,"props":487,"children":489},{"id":488},"新生代空间的-gc-过程是怎样的",[490],{"type":434,"value":491},"新生代空间的 GC 过程是怎样的？",{"type":428,"tag":443,"props":493,"children":494},{},[495],{"type":434,"value":496},"新生代空间通常只有 1-8 MB，被划分为两个等大的区域，出区和入区。新增变量放到入区中。一但入区满了，便把活跃对象复制到出区中，删除不活跃对象（紧缩过程）并交换出区入区，如此称为一个周期。如果活跃对象再两个周期内都没有被清除，则转移只老生代内存中。",{"type":428,"tag":436,"props":498,"children":500},{"id":499},"老生代空间的-gc-过程是怎样的",[501],{"type":434,"value":502},"老生代空间的 GC 过程是怎样的？",{"type":428,"tag":443,"props":504,"children":505},{},[506],{"type":434,"value":507},"老生代空间的 GC 分标记和清除（或紧缩）两个阶段。每次循环，标记算法会将一个对象从队列中取出，将它以及邻接对象标记。到了清除阶段，未被标记的对象都将被删除。如果清楚阶段额外执行紧缩过程，则会尝试将零散的对象移动成连续的整体，以解决内存碎片化问题。为了优化老生代空间 GC 的“全停顿”，V8 将老生代空间的标记过程拆分为了子步骤，这样就可以将标记算法穿插在其它任务过程中执行。",{"type":428,"tag":429,"props":509,"children":511},{"id":510},"api",[512],{"type":434,"value":513},"API",{"type":428,"tag":436,"props":515,"children":517},{"id":516},"nodevm-是什么",[518],{"type":434,"value":519},"node:vm 是什么？",{"type":428,"tag":443,"props":521,"children":522},{},[523],{"type":434,"value":524},"node:vm 模块允许在 V8 虚拟机上下文中编译和运行代码，上下文中的任何属性都被视为全局变量，对全局变量任何改动也反映到上下文对象中。但是 node:vm 不是一个安全机制，不应该使用它来运行不受信任的代码。",{"type":428,"tag":443,"props":526,"children":527},{},[528,530],{"type":434,"value":529},"见：",{"type":428,"tag":531,"props":532,"children":536},"a",{"href":533,"rel":534},"http://nodejs.cn/api/vm.html#vm_vm_executing_javascript",[535],"nofollow",[537],{"type":434,"value":538},"VM",{"type":428,"tag":429,"props":540,"children":542},{"id":541},"源码解析",[543],{"type":434,"value":541},{"type":428,"tag":443,"props":545,"children":546},{},[547],{"type":428,"tag":531,"props":548,"children":550},{"href":549},"/maps/_server/nodejs/source/require",[551],{"type":434,"value":552},"NodeJS | require",{"title":421,"searchDepth":554,"depth":554,"links":555},2,[556,557,558,559],{"id":431,"depth":554,"text":431},{"id":471,"depth":554,"text":474},{"id":510,"depth":554,"text":513},{"id":541,"depth":554,"text":541},"markdown","content:6.maps:_server:nodejs:index.md","content","6.maps/_server/nodejs/index.md","6.maps/_server/nodejs/index","md",[567,571],{"_path":568,"title":569,"description":570},"/maps/_schema/zod","Zod","Zod 是一个 TypeScript-first 的验证库。",{"_path":549,"title":572,"description":573},"NodeJS Require","NodeJS 模块导入的源码解析",1739578923538]