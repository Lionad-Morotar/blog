s",{"title":388,"_path":389},"Windows","/gists/windows",{"title":391,"_path":392},"查缺补漏","/gists/wrong",{"title":394,"_path":395,"children":396},"代码笔记 / Gists","/source-code",[397],{"title":398,"_path":395},"🚄 代码笔记",{"title":400,"_path":401,"children":402},"我 / Abount","/hire",[403],{"title":404,"_path":401},"📬 技术简历",{"title":406,"_path":407,"children":408},"todo","/todo",[409],{"title":406,"_path":407},{"_path":411,"_dir":412,"_draft":413,"_partial":414,"_locale":415,"title":416,"description":415,"body":417,"_type":656,"_id":657,"_source":658,"_file":659,"_stem":660,"_extension":661},"/maps/_computer/computer","_computer",false,true,"","Computer Mind Map",{"type":418,"children":419,"toc":650},"root",[420,428,434,441,456,462,468,527,532,538,543,549,567,581,587,592,615,621,634,639],{"type":421,"tag":422,"props":423,"children":425},"element","h1",{"id":424},"computer-mind-map",[426],{"type":427,"value":416},"text",{"type":421,"tag":429,"props":430,"children":432},"h2",{"id":431},"进制",[433],{"type":427,"value":431},{"type":421,"tag":435,"props":436,"children":438},"h4",{"id":437},"十进制如何转二进制",[439],{"type":427,"value":440},"十进制如何转二进制？",{"type":421,"tag":442,"props":443,"children":444},"ul",{},[445,451],{"type":421,"tag":446,"props":447,"children":448},"li",{},[449],{"type":427,"value":450},"整数除二倒序取余",{"type":421,"tag":446,"props":452,"children":453},{},[454],{"type":427,"value":455},"小数乘二顺序取整",{"type":421,"tag":435,"props":457,"children":459},{"id":458},"十进制转二进制的示例",[460],{"type":427,"value":461},"十进制转二进制的示例？",{"type":421,"tag":463,"props":464,"children":465},"p",{},[466],{"type":427,"value":467},"以 $(4.1)_{10}$ 为例，先算小数部分，再算整数部分：",{"type":421,"tag":469,"props":470,"children":471},"ol",{},[472,477,482,487,492,497,502,507,512,517,522],{"type":421,"tag":446,"props":473,"children":474},{},[475],{"type":427,"value":476},"$0.1;*;2;=;0.2$，得整数位 $0$，取小数位 $0.2$ 继续运算；",{"type":421,"tag":446,"props":478,"children":479},{},[480],{"type":427,"value":481},"$0.2;*;2;=;0.4$，得整数位 $0$，取小数位 $0.4$ 继续运算；",{"type":421,"tag":446,"props":483,"children":484},{},[485],{"type":427,"value":486},"$0.4;*;2;=;0.8$，得整数位 $0$，取小数位 $0.8$ 继续运算；",{"type":421,"tag":446,"props":488,"children":489},{},[490],{"type":427,"value":491},"$0.8;*;2;=;1.6$，得整数位 $1$，取小数位 $0.6$ 继续运算；",{"type":421,"tag":446,"props":493,"children":494},{},[495],{"type":427,"value":496},"$0.6;*;2;=;1.2$，得整数位 $1$，取小数位 $0.2$ 继续运算（从第二步开始循环）；",{"type":421,"tag":446,"props":498,"children":499},{},[500],{"type":427,"value":501},"得 $(0.1)_{10}$ 转为二进制的结果：$(0.0\\overline{0011})_2$",{"type":421,"tag":446,"props":503,"children":504},{},[505],{"type":427,"value":506},"$4;mod;2 = 0$，得余数 $0$，取结果 $4;/;2 = 2$ 继续运算；",{"type":421,"tag":446,"props":508,"children":509},{},[510],{"type":427,"value":511},"$2;mod;2 = 0$，得余数 $0$，取结果 $2;/;2 = 1$ 继续运算；",{"type":421,"tag":446,"props":513,"children":514},{},[515],{"type":427,"value":516},"$1;mod;2 = 1$，得余数 $1$，结束运算；",{"type":421,"tag":446,"props":518,"children":519},{},[520],{"type":427,"value":521},"得 $(4)_{10}$ 的二进制结果：$(100)_2$",{"type":421,"tag":446,"props":523,"children":524},{},[525],{"type":427,"value":526},"得 $(4.1)_{10}$ 的二进制结果：$(100.0\\overline{0011})_2$，可以用 (4.1).toString(2) 验证一下。",{"type":421,"tag":429,"props":528,"children":530},{"id":529},"数字电路",[531],{"type":427,"value":529},{"type":421,"tag":435,"props":533,"children":535},{"id":534},"为什么减法要转换为加法的形式",[536],{"type":427,"value":537},"为什么减法要转换为加法的形式？",{"type":421,"tag":463,"props":539,"children":540},{},[541],{"type":427,"value":542},"负数在用原码表示时，需要在最高位带上符号。但是加减乘法作为最基础的运算，其电路应该设计得简单，识别“符号位”会让基础电路变得复杂，所以需要一种算法来把基础运算转换为二进制加法。使用反码计算减法会带来正负零的问题，使用补码则不会发生异常。",{"type":421,"tag":435,"props":544,"children":546},{"id":545},"什么是原码反码和补码",[547],{"type":427,"value":548},"什么是原码反码和补码？",{"type":421,"tag":442,"props":550,"children":551},{},[552,557,562],{"type":421,"tag":446,"props":553,"children":554},{},[555],{"type":427,"value":556},"最高位为符号位,其余为数值位，这是原码",{"type":421,"tag":446,"props":558,"children":559},{},[560],{"type":427,"value":561},"正数的反码为其本身，负数的反码为值位取反",{"type":421,"tag":446,"props":563,"children":564},{},[565],{"type":427,"value":566},"正数的补码为其本身，负数的补码为其反码加一（符号位不进位）",{"type":421,"tag":463,"props":568,"children":569},{},[570,572],{"type":427,"value":571},"见：",{"type":421,"tag":573,"props":574,"children":578},"a",{"href":575,"rel":576},"http://www.atoolbox.net/Tool.php?Id=952",[577],"nofollow",[579],{"type":427,"value":580},"原码/反码/补码计算器",{"type":421,"tag":435,"props":582,"children":584},{"id":583},"使用补码进行减法的示例",[585],{"type":427,"value":586},"使用补码进行减法的示例？",{"type":421,"tag":463,"props":588,"children":589},{},[590],{"type":427,"value":591},"以四位整形举例如下，计算表达式 $5 - 2$：",{"type":421,"tag":469,"props":593,"children":594},{},[595,600,605,610],{"type":421,"tag":446,"props":596,"children":597},{},[598],{"type":427,"value":599},"将减法用加法表示为 $5 + (-2)$。",{"type":421,"tag":446,"props":601,"children":602},{},[603],{"type":427,"value":604},"$5$ 是正数，直接得补码 $0101$。",{"type":421,"tag":446,"props":606,"children":607},{},[608],{"type":427,"value":609},"$-2$ 的原码是 $1010$，数值位取反得 $1101$，最后数值位加一得补码 $1110$。",{"type":421,"tag":446,"props":611,"children":612},{},[613],{"type":427,"value":614},"补码相加，$0101 + 1110$，得 $0011$，换算回十进制，答案是 $3$。",{"type":421,"tag":435,"props":616,"children":618},{"id":617},"_8-位补码的极值是多少",[619],{"type":427,"value":620},"8 位补码的极值是多少？",{"type":421,"tag":463,"props":622,"children":623},{},[624,626,632],{"type":427,"value":625},"8 位补码正数的最大值是 $01111111$，即 $(127)",{"type":421,"tag":627,"props":628,"children":629},"em",{},[630],{"type":427,"value":631},"{10}$。因为补码没有负零，所以其负数的最小值为 $-(127 - 1)",{"type":427,"value":633},"{10}$，即 $(-128)_{10}$",{"type":421,"tag":429,"props":635,"children":636},{"id":406},[637],{"type":427,"value":638},"TODO",{"type":421,"tag":435,"props":640,"children":642},{"id":641},"原码-反码-补码详解",[643],{"type":421,"tag":573,"props":644,"children":647},{"href":645,"rel":646},"https://www.cnblogs.com/zhangziqiu/archive/2011/03/30/ComputerCode.html",[577],[648],{"type":427,"value":649},"原码, 反码, 补码详解",{"title":415,"searchDepth":651,"depth":651,"links":652},2,[653,654,655],{"id":431,"depth":651,"text":431},{"id":529,"depth":651,"text":529},{"id":406,"depth":651,"text":638},"markdown","content:6.maps:_computer:computer.md","content","6.maps/_computer/computer.md","6.maps/_computer/computer","md",[663,666],{"_path":664,"title":665,"description":415},"/maps/_computer/algorithm","算法",{"_path":667,"title":668,"description":415},"/maps/_computer/network","计算机网络",1732808728270]