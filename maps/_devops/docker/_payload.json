le":388,"_path":389,"children":390},"我 / About","/hire",[391],{"title":392,"_path":389},"📬 技术简历",{"_path":394,"_dir":395,"_draft":396,"_partial":397,"_locale":398,"title":399,"description":400,"body":401,"_type":742,"_id":743,"_source":744,"_file":745,"_stem":746,"_extension":747},"/maps/_devops/docker","_devops",false,true,"","Docker","Docker 是一种容器管理工具，通过对系统资源的控制与隔离，把软件、软件运行时、系统工具、系统库和配置打包到一个镜像中，运行在有有独立命名空间的容器进程内，可以使软件的所有依赖都标准化。",{"type":402,"children":403,"toc":731},"root",[404,412,419,425,431,436,441,462,467,472,477,482,507,512,517,522,527,532,537,542,632,637,673,684,689,700],{"type":405,"tag":406,"props":407,"children":409},"element","h2",{"id":408},"简介",[410],{"type":411,"value":408},"text",{"type":405,"tag":413,"props":414,"children":416},"h4",{"id":415},"docker-是什么",[417],{"type":411,"value":418},"Docker 是什么？",{"type":405,"tag":420,"props":421,"children":422},"p",{},[423],{"type":411,"value":424},"Docker 本身不是容器，而是容器标准的一种实现及其管理工具。",{"type":405,"tag":413,"props":426,"children":428},{"id":427},"容器是什么",[429],{"type":411,"value":430},"容器是什么？",{"type":405,"tag":420,"props":432,"children":433},{},[434],{"type":411,"value":435},"容器是一种软件打包的技术。通过对系统资源的控制与隔离，把软件、软件运行时、系统工具、系统库和配置打包到一个镜像中，运行在有有独立命名空间的容器进程内，可以使软件的所有依赖都标准化。所以使用容器技术能方便地将软件打包成标准单元。",{"type":405,"tag":406,"props":437,"children":439},{"id":438},"原理",[440],{"type":411,"value":438},{"type":405,"tag":420,"props":442,"children":443},{},[444,446,460],{"type":411,"value":445},"Docker 和虚拟机都希望能提供一个虚拟、完整、独立的运行时环境，通过提高虚拟环境的隔离型，以便管理。一般来说，虚拟机的原理是模拟硬件环境，所以需要安装客户端操作系统",{"type":405,"tag":447,"props":448,"children":449},"sup",{},[450],{"type":405,"tag":451,"props":452,"children":457},"a",{"href":453,"ariaDescribedBy":454,"dataFootnoteRef":398,"id":456},"#user-content-fn-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6",[455],"footnote-label","user-content-fnref-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6",[458],{"type":411,"value":459},"1",{"type":411,"value":461},"，而 Docker 并不需要这层客户端操作系统，所以 Docker 更加轻量，硬件利用率也更高。",{"type":405,"tag":420,"props":463,"children":464},{},[465],{"type":411,"value":466},"虚拟机使用 Hypervisor 实现操作系统级别的模拟，而 Docker 通过操作系统的文件系统、命名空间、控制组实现进程级别的模拟。",{"type":405,"tag":420,"props":468,"children":469},{},[470],{"type":411,"value":471},"命名空间：所有 Linux 的进程都从 init 进程 fork 派生。Linux 容器的一个主要组件是新的命名空间下的一个 init 进程，所以我们有能力去创建一个不影响外界的进程树出来。",{"type":405,"tag":420,"props":473,"children":474},{},[475],{"type":411,"value":476},"控制组：通过控制组，我们能限制进程对 CPU、磁盘、内存的使用。",{"type":405,"tag":406,"props":478,"children":480},{"id":479},"概念",[481],{"type":411,"value":479},{"type":405,"tag":483,"props":484,"children":485},"ol",{},[486,492,497,502],{"type":405,"tag":487,"props":488,"children":489},"li",{},[490],{"type":411,"value":491},"隔离环境：虚拟文件系统；POSIX API；文件的只读和屏蔽；命名空间、环境变量等；",{"type":405,"tag":487,"props":493,"children":494},{},[495],{"type":411,"value":496},"生命周期：created、running、pausing、paused、stopped；可以通过 update 动态调整硬件资源使用率",{"type":405,"tag":487,"props":498,"children":499},{},[500],{"type":411,"value":501},"数据卷：数据卷由自己的独立的生命周期",{"type":405,"tag":487,"props":503,"children":504},{},[505],{"type":411,"value":506},"网络：独立的网络命名空间、共享其他实例的网络命名空间、共享主机的网络命名空间",{"type":405,"tag":406,"props":508,"children":510},{"id":509},"架构",[511],{"type":411,"value":509},{"type":405,"tag":420,"props":513,"children":514},{},[515],{"type":411,"value":516},"Docker 引擎实际上由 Dockerd、Containerd、RunC 等组件构成",{"type":405,"tag":420,"props":518,"children":519},{},[520],{"type":411,"value":521},"Dockerd：容器管理 -> 编排管理和集群管理。它接受来自外部的 HTTP 请求并作为 API 调用执行，对内与 Containerd 进行通讯。",{"type":405,"tag":420,"props":523,"children":524},{},[525],{"type":411,"value":526},"Containerd：对镜像、容器的管理提供了一个更进一步的抽象，提供了内容、快照、差异、容器、镜像、任务等更加细粒度的概念。",{"type":405,"tag":420,"props":528,"children":529},{},[530],{"type":411,"value":531},"Container 包含 Containerd-shim，用于正确处理由操作系统发送过来的信号。",{"type":405,"tag":420,"props":533,"children":534},{},[535],{"type":411,"value":536},"RunC：Containerd 提供的容器管理等 API 的执行者是 RunC。",{"type":405,"tag":406,"props":538,"children":540},{"id":539},"常见指令",[541],{"type":411,"value":539},{"type":405,"tag":543,"props":544,"children":545},"ul",{},[546,564,581,598,615],{"type":405,"tag":487,"props":547,"children":548},{},[549,556,558],{"type":405,"tag":550,"props":551,"children":553},"code",{"className":552},[],[554],{"type":411,"value":555},"ps",{"type":411,"value":557},"：列表，如 ",{"type":405,"tag":550,"props":559,"children":561},{"className":560},[],[562],{"type":411,"value":563},"docker ps",{"type":405,"tag":487,"props":565,"children":566},{},[567,573,575],{"type":405,"tag":550,"props":568,"children":570},{"className":569},[],[571],{"type":411,"value":572},"build",{"type":411,"value":574},"：打包镜像，如 ",{"type":405,"tag":550,"props":576,"children":578},{"className":577},[],[579],{"type":411,"value":580},"docker build -t test . -f Dockerfile",{"type":405,"tag":487,"props":582,"children":583},{},[584,590,592],{"type":405,"tag":550,"props":585,"children":587},{"className":586},[],[588],{"type":411,"value":589},"run",{"type":411,"value":591},"：运行镜像，如 ",{"type":405,"tag":550,"props":593,"children":595},{"className":594},[],[596],{"type":411,"value":597},"docker run -it test",{"type":405,"tag":487,"props":599,"children":600},{},[601,607,609],{"type":405,"tag":550,"props":602,"children":604},{"className":603},[],[605],{"type":411,"value":606},"exec",{"type":411,"value":608},"：执行容器指令，如 ",{"type":405,"tag":550,"props":610,"children":612},{"className":611},[],[613],{"type":411,"value":614},"docker exec -it xxx bash",{"type":405,"tag":487,"props":616,"children":617},{},[618,624,626],{"type":405,"tag":550,"props":619,"children":621},{"className":620},[],[622],{"type":411,"value":623},"copy",{"type":411,"value":625},"：复制文件到 Docker 中，如 ",{"type":405,"tag":550,"props":627,"children":629},{"className":628},[],[630],{"type":411,"value":631},"docker cp d://test dockerID:dockerPath",{"type":405,"tag":406,"props":633,"children":635},{"id":634},"调试流程",[636],{"type":411,"value":634},{"type":405,"tag":483,"props":638,"children":639},{},[640,651,662],{"type":405,"tag":487,"props":641,"children":642},{},[643,645],{"type":411,"value":644},"拉远端镜像：",{"type":405,"tag":550,"props":646,"children":648},{"className":647},[],[649],{"type":411,"value":650},"docker pull registry.gitlab.p-ray.cn/moe:75150",{"type":405,"tag":487,"props":652,"children":653},{},[654,656],{"type":411,"value":655},"启动容器：",{"type":405,"tag":550,"props":657,"children":659},{"className":658},[],[660],{"type":411,"value":661},"docker run -p 8081:7001 -it \u003CimageID> sleep 999m",{"type":405,"tag":487,"props":663,"children":664},{},[665,667],{"type":411,"value":666},"进入并调试：",{"type":405,"tag":550,"props":668,"children":670},{"className":669},[],[671],{"type":411,"value":672},"docker exec -it \u003CinstanceName> /bin/bash",{"type":405,"tag":420,"props":674,"children":675},{},[676,682],{"type":405,"tag":550,"props":677,"children":679},{"className":678},[],[680],{"type":411,"value":681},"-p 8081:7001",{"type":411,"value":683},"，把本机的 8081 端口绑定到 Container 的 7001 端口。",{"type":405,"tag":406,"props":685,"children":687},{"id":686},"常见问题",[688],{"type":411,"value":686},{"type":405,"tag":420,"props":690,"children":691},{},[692,694],{"type":411,"value":693},"关闭 Hyper-V：",{"type":405,"tag":550,"props":695,"children":697},{"className":696},[],[698],{"type":411,"value":699},"bcdedit /set hypervisorlaunchtype off",{"type":405,"tag":701,"props":702,"children":705},"section",{"className":703,"dataFootnotes":398},[704],"footnotes",[706,713],{"type":405,"tag":406,"props":707,"children":710},{"className":708,"id":455},[709],"sr-only",[711],{"type":411,"value":712},"Footnotes",{"type":405,"tag":483,"props":714,"children":715},{},[716],{"type":405,"tag":487,"props":717,"children":719},{"id":718},"user-content-fn-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6",[720,722],{"type":411,"value":721},"虚拟机一般有三种模拟方式，裸机型（硬件 + 虚拟 OS + 虚拟 APP）、托管型（OS + Hypervisor + 虚拟 OS + 虚拟 APP）和操作系统虚拟化（OS + 容器 + 虚拟 APP）。 ",{"type":405,"tag":451,"props":723,"children":728},{"href":724,"ariaLabel":725,"className":726,"dataFootnoteBackref":398},"#user-content-fnref-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6","Back to reference 1",[727],"data-footnote-backref",[729],{"type":411,"value":730},"↩",{"title":398,"searchDepth":732,"depth":732,"links":733},2,[734,735,736,737,738,739,740,741],{"id":408,"depth":732,"text":408},{"id":438,"depth":732,"text":438},{"id":479,"depth":732,"text":479},{"id":509,"depth":732,"text":509},{"id":539,"depth":732,"text":539},{"id":634,"depth":732,"text":634},{"id":686,"depth":732,"text":686},{"id":455,"depth":732,"text":712},"markdown","content:6.maps:_devops:docker.md","content","6.maps/_devops/docker.md","6.maps/_devops/docker","md",[749,753],{"_path":750,"title":751,"description":752},"/maps/_devops/devops","DevOps","DevOps 是一种开发理论，它致力于将大量自动化操作整合进开发到交付过程中，以消除各个团队（如开发、安全、运维）之间的隔阂，使需求尽快交付，并得到质量反馈。",{"_path":754,"title":755,"description":756},"/maps/_devops/git/dorothy","Dorothy","Dorothy 是我的 Git Commit Message 的约定。",1741604964684]