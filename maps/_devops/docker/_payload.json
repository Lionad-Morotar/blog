/vpn",{"title":388,"_path":389},"VS Code 插件开发","/gists/vscode-plugin",{"title":391,"_path":392},"Web Components","/gists/web-components",{"title":394,"_path":395},"Windows","/gists/windows",{"title":397,"_path":398},"查缺补漏","/gists/wrong",{"title":400,"_path":401,"children":402},"代码笔记 / Gists","/source-code",[403],{"title":404,"_path":401},"🚄 代码笔记",{"title":406,"_path":407,"children":408},"我 / Abount","/hire",[409],{"title":410,"_path":407},"📬 技术简历",{"title":412,"_path":413,"children":414},"todo","/todo",[415],{"title":412,"_path":413},{"_path":417,"_dir":418,"_draft":419,"_partial":420,"_locale":421,"title":422,"description":423,"body":424,"_type":765,"_id":766,"_source":767,"_file":768,"_stem":769,"_extension":770},"/maps/_devops/docker","_devops",false,true,"","Docker","Docker 是一种容器管理工具，通过对系统资源的控制与隔离，把软件、软件运行时、系统工具、系统库和配置打包到一个镜像中，运行在有有独立命名空间的容器进程内，可以使软件的所有依赖都标准化。",{"type":425,"children":426,"toc":754},"root",[427,435,442,448,454,459,464,485,490,495,500,505,530,535,540,545,550,555,560,565,655,660,696,707,712,723],{"type":428,"tag":429,"props":430,"children":432},"element","h2",{"id":431},"简介",[433],{"type":434,"value":431},"text",{"type":428,"tag":436,"props":437,"children":439},"h4",{"id":438},"docker-是什么",[440],{"type":434,"value":441},"Docker 是什么？",{"type":428,"tag":443,"props":444,"children":445},"p",{},[446],{"type":434,"value":447},"Docker 本身不是容器，而是容器标准的一种实现及其管理工具。",{"type":428,"tag":436,"props":449,"children":451},{"id":450},"容器是什么",[452],{"type":434,"value":453},"容器是什么？",{"type":428,"tag":443,"props":455,"children":456},{},[457],{"type":434,"value":458},"容器是一种软件打包的技术。通过对系统资源的控制与隔离，把软件、软件运行时、系统工具、系统库和配置打包到一个镜像中，运行在有有独立命名空间的容器进程内，可以使软件的所有依赖都标准化。所以使用容器技术能方便地将软件打包成标准单元。",{"type":428,"tag":429,"props":460,"children":462},{"id":461},"原理",[463],{"type":434,"value":461},{"type":428,"tag":443,"props":465,"children":466},{},[467,469,483],{"type":434,"value":468},"Docker 和虚拟机都希望能提供一个虚拟、完整、独立的运行时环境，通过提高虚拟环境的隔离型，以便管理。一般来说，虚拟机的原理是模拟硬件环境，所以需要安装客户端操作系统",{"type":428,"tag":470,"props":471,"children":472},"sup",{},[473],{"type":428,"tag":474,"props":475,"children":480},"a",{"href":476,"ariaDescribedBy":477,"dataFootnoteRef":421,"id":479},"#user-content-fn-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6",[478],"footnote-label","user-content-fnref-%E6%A8%A1%E6%8B%9F%E7%A1%AC%E4%BB%B6",[481],{"type":434,"value":482},"1",{"type":434,"value":484},"，而 Docker 并不需要这层客户端操作系统，所以 Docker 更加轻量，硬件利用率也更高。",{"type":428,"tag":443,"props":486,"children":487},{},[488],{"type":434,"value":489},"虚拟机使用 Hypervisor 实现操作系统级别的模拟，而 Docker 通过操作系统的文件系统、命名空间、控制组实现进程级别的模拟。",{"type":428,"tag":443,"props":491,"children":492},{},[493],{"type":434,"value":494},"命名空间：所有 Linux 的进程都从 init 进程 fork 派生。Linux 容器的一个主要组件是新的命名空间下的一个 init 进程，所以我们有能力去创建一个不影响外界的进程树出来。",{"type":428,"tag":443,"props":496,"children":497},{},[498],{"type":434,"value":499},"控制组：通过控制组，我们能限制进程对 CPU、磁盘、内存的使用。",{"type":428,"tag":429,"props":501,"children":503},{"id":502},"概念",[504],{"type":434,"value":502},{"type":428,"tag":506,"props":507,"children":508},"ol",{},[509,515,520,525],{"type":428,"tag":510,"props":511,"children":512},"li",{},[513],{"type":434,"value":514},"隔离环境：虚拟文件系统；POSIX API；文件的只读和屏蔽；命名空间、环境变量等；",{"type":428,"tag":510,"props":516,"children":517},{},[518],{"type":434,"value":519},"生命周期：created、running、pausing、paused、stopped；可以通过 update 动态调整硬件资源使用率",{"type":428,"tag":510,"props":521,"children":522},{},[523],{"type":434,"value":524},"数据卷：数据卷由自己的独立的生命周期",{"type":428,"tag":510,"props":526,"children":527},{},[528],{"type":434,"value":529},"网络：独立的网络命名空间、共享其他实例的网络命名空间、共享主机的网络命名空间",{"type":428,"tag":429,"props":531,"children":533},{"id":532},"架构",[534],{"type":434,"value":532},{"type":428,"tag":443,"props":536,"children":537},{},[538],{"type":434,"value":539},"Docker 引擎实际上由 Dockerd、Containerd、RunC 等组件构成",{"type":428,"tag":443,"props":541,"children":542},{},[543],{"type":434,"value":544},"Dockerd：容器管理 -> 编排管理和集群管理。它接受来自外部的 HTTP 请求并作为 API 调用执行，对内与 Containerd 进行通讯。",{"type":428,"tag":443,"props":546,"children":547},{},[548],{"type":434,"value":549},"Containerd：对镜像、容器的管理提供了一个更进一步的抽象，提供了内容、快照、差异、容器、镜像、任务等更加细粒度的概念。",{"type":428,"tag":443,"props":551,"children":552},{},[553],{"type":434,"value":554},"Container 包含 Containerd-shim，用于正确处理由操作系统发送过来的信号。",{"type":428,"tag":443,"props":556,"children":557},{},[558],{"type":434,"value":559},"RunC：Containerd 提供的容器管理等 API 的执行者是 RunC。",{"type":428,"tag":429,"props":561,"children":563},{"id":562},"常见指令",[564],{"type":434,"value":562},{"type":428,"tag":566,"props":567,"children":568},"ul",{},[569,587,604,621,638],{"type":428,"tag":510,"props":570,"children":571},{},[572,579,581],{"type":428,"tag":573,"props":574,"children":576},"code",{"className":575},[],[577],{"type":434,"value":578},"ps",{"type":434,"value":580},"：列表，如 ",{"type":428,"tag":573,"props":582,"children":584},{"className":583},[],[585],{"type":434,"value":586},"docker ps",{"type":428,"tag":510,"props":588,"children":589},{},[590,596,598],{"type":428,"tag":573,"props":591,"children":593},{"className":592},[],[594],{"type":434,"value":595},"build",{"type":434,"value":597},"：打包镜像，如 ",{"type":428,"tag":573,"props":599,"children":601},{"className":600},[],[602],{"type":434,"value":603},"docker build -t test . -f Dockerfile",{"type":428,"tag":510,"props":605,"children":606},{},[607,613,615],{"type":428,"tag":573,"props":608,"children":610},{"className":609},[],[611],{"type":434,"value":612},"run",{"type":434,"value":614},"：运行镜像，如 ",{"type":428,"tag":573,"props":616,"children":618},{"className":617},[],[619],{"type":434,"value":620},"docker run -it test",{"type":428,"tag":510,"props":622,"children":623},{},[624,630,632],{"type":428,"tag":573,"props":625,"children":627},{"className":626},[],[628],{"type":434,"value":629},"exec",{"type":434,"value":631},"：执行容器指令，如 ",{"type":428,"tag":573,"props":633,"children":635},{"className":634},[],[636],{"type":434,"value":637},"docker exec -it xxx bash",{"type":428,"tag":510,"props":639,"children":640},{},[641,647,649],{"type":428,"tag":573,"props":6