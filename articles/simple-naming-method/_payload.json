8,"value":366},"Instead of picking random numbers, map the ",{"type":173,"tag":212,"props":368,"children":369},{},[370],{"type":178,"value":371},"project name to numbers based on the keyboard",{"type":178,"value":373},", so the port is ",{"type":173,"tag":331,"props":375,"children":376},{},[377],{"type":178,"value":378},"readable",{"type":178,"value":380}," and ",{"type":173,"tag":331,"props":382,"children":383},{},[384],{"type":178,"value":385},"memorable",{"type":178,"value":387},".",{"type":173,"tag":174,"props":389,"children":390},{},[391,393,398],{"type":178,"value":392},"As long as the result is within the valid port range (",{"type":173,"tag":212,"props":394,"children":395},{},[396],{"type":178,"value":397},"0â€“65535",{"type":178,"value":399},") and doesnâ€™t hit reserved/system ports, you can just use it.",{"type":173,"tag":174,"props":401,"children":402},{},[403,405,410],{"type":178,"value":404},"More specifically: using a standard QWERTY keyboard, map each letter to a single digit based on its ",{"type":173,"tag":212,"props":406,"children":407},{},[408],{"type":178,"value":409},"row/column position",{"type":178,"value":387},{"type":173,"tag":174,"props":412,"children":413},{},[414],{"type":178,"value":415},"Example:",{"type":173,"tag":174,"props":417,"children":418},{},[419,425,427],{"type":173,"tag":237,"props":420,"children":422},{"className":421},[],[423],{"type":178,"value":424},"\"cfetch\"",{"type":178,"value":426}," â†’ ",{"type":173,"tag":237,"props":428,"children":430},{"className":429},[],[431],{"type":178,"value":242},{"type":173,"tag":174,"props":433,"children":434},{},[435,437,443,445,451],{"type":178,"value":436},"Then you can take the first 4 digits (e.g. ",{"type":173,"tag":237,"props":438,"children":440},{"className":439},[],[441],{"type":178,"value":442},"3453",{"type":178,"value":444},"), or keep more digits (e.g. ",{"type":173,"tag":237,"props":446,"children":448},{"className":447},[],[449],{"type":178,"value":450},"34353",{"type":178,"value":452},"). Either is fine.",{"type":173,"tag":174,"props":454,"children":455},{},[456,458,463],{"type":178,"value":457},"If a project needs multiple ports (frontend, backend, database, etc.), pick ",{"type":173,"tag":212,"props":459,"children":460},{},[461],{"type":178,"value":462},"one",{"type":178,"value":464}," of these two approaches:",{"type":173,"tag":251,"props":466,"children":467},{},[468,564],{"type":173,"tag":185,"props":469,"children":470},{},[471,473],{"type":178,"value":472},"Use the project prefix, then append a â€œrole suffixâ€",{"type":173,"tag":181,"props":474,"children":475},{},[476,496,523,541,559],{"type":173,"tag":185,"props":477,"children":478},{},[479,481,486,488,494],{"type":178,"value":480},"For ",{"type":173,"tag":237,"props":482,"children":484},{"className":483},[],[485],{"type":178,"value":424},{"type":178,"value":487},", take ",{"type":173,"tag":237,"props":489,"children":491},{"className":490},[],[492],{"type":178,"value":493},"3435",{"type":178,"value":495}," as the base",{"type":173,"tag":185,"props":497,"children":498},{},[499,501,507,509,515,517],{"type":178,"value":500},"Frontend (",{"type":173,"tag":237,"props":502,"children":504},{"className":503},[],[505],{"type":178,"value":506},"fe",{"type":178,"value":508},", i.e. ",{"type":173,"tag":237,"props":510,"children":512},{"className":511},[],[513],{"type":178,"value":514},"43",{"type":178,"value":516},") â†’ ",{"type":173,"tag":237,"props":518,"children":520},{"className":519},[],[521],{"type":178,"value":522},"34354",{"type":173,"tag":185,"props":524,"children":525},{},[526,528,534,535],{"type":178,"value":527},"Backend (",{"type":173,"tag":237,"props":529,"children":531},{"className":530},[],[532],{"type":178,"value":533},"server",{"type":178,"value":516},{"type":173,"tag":237,"props":536,"children":538},{"className":537},[],[539],{"type":178,"value":540},"34352",{"type":173,"tag":185,"props":542,"children":543},{},[544,546,552,553],{"type":178,"value":545},"Database (",{"ty