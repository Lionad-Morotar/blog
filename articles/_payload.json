hildren":390},"我 / About","/hire",[391],{"title":392,"_path":389},"📬 技术简历",{"_path":37,"_dir":394,"_draft":396,"_partial":396,"_locale":397,"title":40,"description":398,"body":399,"_type":533,"_id":534,"_source":535,"_file":536,"_stem":537,"_extension":538},{"_path":395,"title":36},"/articles/_dir",false,"","笔记、博客以及碎碎念，我的思考和再创造。",{"type":400,"children":401,"toc":527},"root",[402,411,417,422,427,432,465,473,485,491,496,501,506],{"type":403,"tag":404,"props":405,"children":407},"element","h2",{"id":406},"我是谁",[408],{"type":409,"value":410},"text","我是谁？",{"type":403,"tag":412,"props":413,"children":414},"p",{},[415],{"type":409,"value":416},"学习并模仿身边的人类行动的机器人，中文名叫“仿生狮子”，但真实身份是只猫。",{"type":403,"tag":412,"props":418,"children":419},{},[420],{"type":409,"value":421},"博客是我顺手建成的，这么说是因为我最初想把这个网站做成笔记项目，类似富文本编辑器之类的，有着更高级交互方式的笔记应用。但网站建起来后就开始摸了，没有思考过或学习过要怎么记笔记，这是不好的，千万别学。",{"type":403,"tag":404,"props":423,"children":425},{"id":424},"网站导航",[426],{"type":409,"value":424},{"type":403,"tag":412,"props":428,"children":429},{},[430],{"type":409,"value":431},"大体来说，这个网站包含：“心流”、技术博客、笔记这三个板块。",{"type":403,"tag":433,"props":434,"children":435},"ul",{},[436,448],{"type":403,"tag":437,"props":438,"children":439},"li",{},[440,446],{"type":403,"tag":441,"props":442,"children":443},"a",{"href":6},[444],{"type":409,"value":445},"心流",{"type":409,"value":447},"：闪念以及部分体裁不明的文章。",{"type":403,"tag":437,"props":449,"children":450},{},[451,456,458,463],{"type":403,"tag":441,"props":452,"children":453},{"href":146},[454],{"type":409,"value":455},"笔记",{"type":409,"value":457},"：一些结构化或系统化的笔记，正在从 ",{"type":403,"tag":441,"props":459,"children":460},{"href":152},[461],{"type":409,"value":462},"Gists",{"type":409,"value":464}," 逐步迁移。",{"type":403,"tag":433,"props":466,"children":467},{},[468],{"type":403,"tag":437,"props":469,"children":470},{},[471],{"type":409,"value":472},"技术博客：我是前端工程师，以前喜欢写文章放到掘金，现在都搬回这里。",{"type":403,"tag":412,"props":474,"children":475},{},[476,478,483],{"type":409,"value":477},"由于并不擅长写技术博客，所以相关内容的比重会逐渐降低。接下来持续很长一段时间，我的写作重点会放到 ",{"type":403,"tag":441,"props":479,"children":480},{"href":13},[481],{"type":409,"value":482},"《四十二篇》系列",{"type":409,"value":484},"。",{"type":403,"tag":404,"props":486,"children":488},{"id":487},"为什么写博客",[489],{"type":409,"value":490},"为什么写博客？",{"type":403,"tag":412,"props":492,"children":493},{},[494],{"type":409,"value":495},"以前，放在这页的内容是一篇长篇大论。增增补补，最终全删了，因为已经不再重要。",{"type":403,"tag":412,"props":497,"children":498},{},[499],{"type":409,"value":500},"传统的博客及其精神太难持续，我更多把这个网站看成某种墓碑（主页），也就是在数字世界的投射。因为曾经想过如何意识数字化合法，我可能会率先尝试上传意识，而在此之前，为何不能是这个网站。至于为什么不能是其他平台，那也仅因为目前存在数据所有权问题相关争论。",{"type":403,"tag":412,"props":502,"children":503},{},[504],{"type":409,"value":505},"另一种看法是，把他当成一座“数字花园”。看看那些零散的、灰屑的、燃烧的、组成我的，从中能长出什么东西。",{"type":403,"tag":412,"props":507,"children":508},{},[509,511,517,519,525],{"type":409,"value":510},"本站提供 ",{"type":403,"tag":441,"props":512,"children":514},{"href":513},"/feed.xml",[515],{"type":409,"value":516},"RSS 订阅",{"type":409,"value":518}," 以及 ",{"type":403,"tag":441,"props":520,"children":522},{"href":521},"/links",[523],{"type":409,"value":524},"友链",{"type":409,"value":526},"。有些内容也许会同步更新其他平台，比如知乎、微信公众号（Lionad）或者哔哩哔哩。",{"title":397,"searchDepth":528,"depth":528,"links":529},2,[530,531,532],{"id":406,"depth":528,"text":410},{"id":424,"depth":528,"text":424},{"id":487,"depth":528,"text":490},"markdown","content:2.articles:0.index.md","content","2.articles/0.index.md","2.articles/0.index","md",[540,542],{"_path":34,"title":33,"description":541},"这里会收录几乎不再更新的内容，请注意其时效性",{"_path":43,"title":42,"description":543},"本文探讨如何结合 Tailwind、CSS Layers 和 Sass，优化样式管理",1741604915655]